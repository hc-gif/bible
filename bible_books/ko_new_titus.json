[{"bible_type": 1, "lang": "ko", "chapter": "1", "line": 1, "content": "\ud558\ub098\ub2d8\uc758 \uc885\uc774\uc694 \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc0ac\ub3c4\uc778 \ubc14\uc6b8 \uace7 \ub098\uc758 \uc0ac\ub3c4 \ub41c \uac83\uc740 \ud558\ub098\ub2d8\uc758 \ud0dd\ud558\uc2e0 \uc790\ub4e4\uc758 \ubbff\uc74c\uacfc \uacbd\uac74\ud568\uc5d0 \uc18d\ud55c \uc9c4\ub9ac\uc758 \uc9c0\uc2dd\uacfc", "content_old": "1 \ud558\ub098\ub2d8\uc758 \uc885\uc774\uc694 \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc0ac\ub3c4\uc778 \ubc14\uc6b8 \uace7 \ub098\uc758 \uc0ac\ub3c4 \ub41c \uac83\uc740 \ud558\ub098\ub2d8\uc758 \ud0dd\ud558\uc2e0 \uc790\ub4e4\uc758 \ubbff\uc74c\uacfc \uacbd\uac74\ud568\uc5d0 \uc18d\ud55c \uc9c4\ub9ac\uc758 \uc9c0\uc2dd\uacfc", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 2, "content": "\uc601\uc0dd\uc758 \uc18c\ub9dd\uc744 \uc778\ud568\uc774\ub77c \uc774 \uc601\uc0dd\uc740 \uac70\uc9d3\uc774 \uc5c6\uc73c\uc2e0 \ud558\ub098\ub2d8\uc774 \uc601\uc6d0\ud55c \ub54c \uc804\ubd80\ud130 \uc57d\uc18d\ud558\uc2e0 \uac83\uc778\ub370", "content_old": "2 \uc601\uc0dd\uc758 \uc18c\ub9dd\uc744 \uc778\ud568\uc774\ub77c \uc774 \uc601\uc0dd\uc740 \uac70\uc9d3\uc774 \uc5c6\uc73c\uc2e0 \ud558\ub098\ub2d8\uc774 \uc601\uc6d0\ud55c \ub54c \uc804\ubd80\ud130 \uc57d\uc18d\ud558\uc2e0 \uac83\uc778\ub370", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 3, "content": "\uc790\uae30 \ub54c\uc5d0 \uc790\uae30\uc758 \ub9d0\uc500\uc744 \uc804\ub3c4\ub85c \ub098\ud0c0\ub0b4\uc168\uc73c\ub2c8 \uc774 \uc804\ub3c4\ub294 \uc6b0\ub9ac \uad6c\uc8fc \ud558\ub098\ub2d8\uc758 \uba85\ub300\ub85c \ub0b4\uac8c \ub9e1\uae30\uc2e0 \uac83\uc774\ub77c", "content_old": "3 \uc790\uae30 \ub54c\uc5d0 \uc790\uae30\uc758 \ub9d0\uc500\uc744 \uc804\ub3c4\ub85c \ub098\ud0c0\ub0b4\uc168\uc73c\ub2c8 \uc774 \uc804\ub3c4\ub294 \uc6b0\ub9ac \uad6c\uc8fc \ud558\ub098\ub2d8\uc758 \uba85\ub300\ub85c \ub0b4\uac8c \ub9e1\uae30\uc2e0 \uac83\uc774\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 4, "content": "\uac19\uc740 \ubbff\uc74c\uc744 \ub530\ub77c \ub41c \ub098\uc758 \ucc38 \uc544\ub4e4 \ub514\ub3c4\uc5d0\uac8c \ud3b8\uc9c0\ud558\ub178\ub2c8 \ud558\ub098\ub2d8 \uc544\ubc84\uc9c0\uc640 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218 \uc6b0\ub9ac \uad6c\uc8fc\ub85c \uc887\uc544 \uc740\ud61c\uc640 \ud3c9\uac15\uc774 \ub124\uac8c \uc788\uc744\uc9c0\uc5b4\ub2e4", "content_old": "4 \uac19\uc740 \ubbff\uc74c\uc744 \ub530\ub77c \ub41c \ub098\uc758 \ucc38 \uc544\ub4e4 \ub514\ub3c4\uc5d0\uac8c \ud3b8\uc9c0\ud558\ub178\ub2c8 \ud558\ub098\ub2d8 \uc544\ubc84\uc9c0\uc640 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218 \uc6b0\ub9ac \uad6c\uc8fc\ub85c \uc887\uc544 \uc740\ud61c\uc640 \ud3c9\uac15\uc774 \ub124\uac8c \uc788\uc744\uc9c0\uc5b4\ub2e4", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 5, "content": "\ub0b4\uac00 \ub108\ub97c \uadf8\ub808\ub370\uc5d0 \ub5a8\uc5b4\ub728\ub824 \ub454 \uc774\uc720\ub294 \ubd80\uc871\ud55c \uc77c\uc744 \ubc14\ub85c\uc7a1\uace0 \ub098\uc758 \uba85\ud55c\ub300\ub85c \uac01 \uc131\uc5d0 \uc7a5\ub85c\ub4e4\uc744 \uc138\uc6b0\uac8c \ud558\ub824 \ud568\uc774\ub2c8", "content_old": "5 \ub0b4\uac00 \ub108\ub97c \uadf8\ub808\ub370\uc5d0 \ub5a8\uc5b4\ub728\ub824 \ub454 \uc774\uc720\ub294 \ubd80\uc871\ud55c \uc77c\uc744 \ubc14\ub85c\uc7a1\uace0 \ub098\uc758 \uba85\ud55c\ub300\ub85c \uac01 \uc131\uc5d0 \uc7a5\ub85c\ub4e4\uc744 \uc138\uc6b0\uac8c \ud558\ub824 \ud568\uc774\ub2c8", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 6, "content": "\ucc45\ub9dd\ud560 \uac83\uc774 \uc5c6\uace0 \ud55c \uc544\ub0b4\uc758 \ub0a8\ud3b8\uc774\uba70 \ubc29\ud0d5\ud558\ub2e4 \ud558\ub294 \ube44\ubc29\uc774\ub098 \ubd88\uc21c\uc885\ud558\ub294 \uc77c\uc774 \uc5c6\ub294 \ubbff\ub294 \uc790\ub140\ub97c \ub454 \uc790\ub77c\uc57c \ud560\uc9c0\ub77c", "content_old": "6 \ucc45\ub9dd\ud560 \uac83\uc774 \uc5c6\uace0 \ud55c \uc544\ub0b4\uc758 \ub0a8\ud3b8\uc774\uba70 \ubc29\ud0d5\ud558\ub2e4 \ud558\ub294 \ube44\ubc29\uc774\ub098 \ubd88\uc21c\uc885\ud558\ub294 \uc77c\uc774 \uc5c6\ub294 \ubbff\ub294 \uc790\ub140\ub97c \ub454 \uc790\ub77c\uc57c \ud560\uc9c0\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 7, "content": "\uac10\ub3c5\uc740 \ud558\ub098\ub2d8\uc758 \uccad\uc9c0\uae30\ub85c\uc11c \ucc45\ub9dd\ud560 \uac83\uc774 \uc5c6\uace0 \uc81c \uace0\uc9d1\ub300\ub85c \ud558\uc9c0 \uc544\ub2c8\ud558\uba70 \uae09\ud788 \ubd84\ub0b4\uc9c0 \uc544\ub2c8\ud558\uba70 \uc220\uc744 \uc990\uae30\uc9c0 \uc544\ub2c8\ud558\uba70 \uad6c\ud0c0\ud558\uc9c0 \uc544\ub2c8\ud558\uba70 \ub354\ub7ec\uc6b4 \uc774\ub97c \ud0d0\ud558\uc9c0 \uc544\ub2c8\ud558\uba70", "content_old": "7 \uac10\ub3c5\uc740 \ud558\ub098\ub2d8\uc758 \uccad\uc9c0\uae30\ub85c\uc11c \ucc45\ub9dd\ud560 \uac83\uc774 \uc5c6\uace0 \uc81c \uace0\uc9d1\ub300\ub85c \ud558\uc9c0 \uc544\ub2c8\ud558\uba70 \uae09\ud788 \ubd84\ub0b4\uc9c0 \uc544\ub2c8\ud558\uba70 \uc220\uc744 \uc990\uae30\uc9c0 \uc544\ub2c8\ud558\uba70 \uad6c\ud0c0\ud558\uc9c0 \uc544\ub2c8\ud558\uba70 \ub354\ub7ec\uc6b4 \uc774\ub97c \ud0d0\ud558\uc9c0 \uc544\ub2c8\ud558\uba70", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 8, "content": "\uc624\uc9c1 \ub098\uadf8\ub124\ub97c \ub300\uc811\ud558\uba70 \uc120\uc744 \uc88b\uc544\ud558\uba70 \uadfc\uc2e0\ud558\uba70 \uc758\ub85c\uc6b0\uba70 \uac70\ub8e9\ud558\uba70 \uc808\uc81c\ud558\uba70", "content_old": "8 \uc624\uc9c1 \ub098\uadf8\ub124\ub97c \ub300\uc811\ud558\uba70 \uc120\uc744 \uc88b\uc544\ud558\uba70 \uadfc\uc2e0\ud558\uba70 \uc758\ub85c\uc6b0\uba70 \uac70\ub8e9\ud558\uba70 \uc808\uc81c\ud558\uba70", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 9, "content": "\ubbf8\uc05c \ub9d0\uc500\uc758 \uac00\ub974\uce68\uc744 \uadf8\ub300\ub85c \uc9c0\ucf1c\uc57c \ud558\ub9ac\ub2c8 \uc774\ub294 \ub2a5\ud788 \ubc14\ub978 \uad50\ud6c8\uc73c\ub85c \uad8c\uba74\ud558\uace0 \uac70\uc2a4\ub824 \ub9d0\ud558\ub294 \uc790\ub4e4\uc744 \ucc45\ub9dd\ud558\uac8c \ud558\ub824 \ud568\uc774\ub77c", "content_old": "9 \ubbf8\uc05c \ub9d0\uc500\uc758 \uac00\ub974\uce68\uc744 \uadf8\ub300\ub85c \uc9c0\ucf1c\uc57c \ud558\ub9ac\ub2c8 \uc774\ub294 \ub2a5\ud788 \ubc14\ub978 \uad50\ud6c8\uc73c\ub85c \uad8c\uba74\ud558\uace0 \uac70\uc2a4\ub824 \ub9d0\ud558\ub294 \uc790\ub4e4\uc744 \ucc45\ub9dd\ud558\uac8c \ud558\ub824 \ud568\uc774\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 10, "content": "\ubcf5\uc885\uce58 \uc544\ub2c8\ud558\uace0 \ud5db\ub41c \ub9d0\uc744 \ud558\uba70 \uc18d\uc774\ub294 \uc790\uac00 \ub9ce\uc740 \uc911 \ud2b9\ubcc4\ud788 \ud560\ub840\ub2f9 \uac00\uc6b4\ub370 \uc2ec\ud558\ub2c8", "content_old": "10 \ubcf5\uc885\uce58 \uc544\ub2c8\ud558\uace0 \ud5db\ub41c \ub9d0\uc744 \ud558\uba70 \uc18d\uc774\ub294 \uc790\uac00 \ub9ce\uc740 \uc911 \ud2b9\ubcc4\ud788 \ud560\ub840\ub2f9 \uac00\uc6b4\ub370 \uc2ec\ud558\ub2c8", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 11, "content": "\uc800\ud76c\uc758 \uc785\uc744 \ub9c9\uc744 \uac83\uc774\ub77c \uc774\ub7f0 \uc790\ub4e4\uc774 \ub354\ub7ec\uc6b4 \uc774\ub97c \ucde8\ud558\ub824\uace0 \ub9c8\ub545\uce58 \uc544\ub2c8\ud55c \uac83\uc744 \uac00\ub974\uccd0 \uc9d1\ub4e4\uc744 \uc628\ud1b5 \uc5ce\ub4dc\ub7ec\uce58\ub294\ub3c4\ub2e4", "content_old": "11 \uc800\ud76c\uc758 \uc785\uc744 \ub9c9\uc744 \uac83\uc774\ub77c \uc774\ub7f0 \uc790\ub4e4\uc774 \ub354\ub7ec\uc6b4 \uc774\ub97c \ucde8\ud558\ub824\uace0 \ub9c8\ub545\uce58 \uc544\ub2c8\ud55c \uac83\uc744 \uac00\ub974\uccd0 \uc9d1\ub4e4\uc744 \uc628\ud1b5 \uc5ce\ub4dc\ub7ec\uce58\ub294\ub3c4\ub2e4", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 12, "content": "\uadf8\ub808\ub370\uc778 \uc911\uc5d0 \uc5b4\ub5a4 \uc120\uc9c0\uc790\uac00 \ub9d0\ud558\ub418 \uadf8\ub808\ub370\uc778\ub4e4\uc740 \ud56d\uc0c1 \uac70\uc9d3\ub9d0\uc7a5\uc774\uba70 \uc545\ud55c \uc9d0\uc2b9\uc774\uba70 \ubc30\ub9cc \uc704\ud558\ub294 \uac8c\uc73c\ub984\uc7a5\uc774\ub77c \ud558\ub2c8", "content_old": "12 \uadf8\ub808\ub370\uc778 \uc911\uc5d0 \uc5b4\ub5a4 \uc120\uc9c0\uc790\uac00 \ub9d0\ud558\ub418 \uadf8\ub808\ub370\uc778\ub4e4\uc740 \ud56d\uc0c1 \uac70\uc9d3\ub9d0\uc7a5\uc774\uba70 \uc545\ud55c \uc9d0\uc2b9\uc774\uba70 \ubc30\ub9cc \uc704\ud558\ub294 \uac8c\uc73c\ub984\uc7a5\uc774\ub77c \ud558\ub2c8", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 13, "content": "\uc774 \uc99d\uac70\uac00 \ucc38\ub418\ub3c4\ub2e4 \uadf8\ub7ec\ubbc0\ub85c \ub124\uac00 \uc800\ud76c\ub97c \uc5c4\ud788 \uafb8\uc9d6\uc73c\ub77c \uc774\ub294 \uc800\ud76c\ub85c \ud558\uc5ec\uae08 \ubbff\uc74c\uc744 \uc628\uc804\ucf00 \ud558\uace0", "content_old": "13 \uc774 \uc99d\uac70\uac00 \ucc38\ub418\ub3c4\ub2e4 \uadf8\ub7ec\ubbc0\ub85c \ub124\uac00 \uc800\ud76c\ub97c \uc5c4\ud788 \uafb8\uc9d6\uc73c\ub77c \uc774\ub294 \uc800\ud76c\ub85c \ud558\uc5ec\uae08 \ubbff\uc74c\uc744 \uc628\uc804\ucf00 \ud558\uace0", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 14, "content": "\uc720\ub300\uc778\uc758 \ud5c8\ud0c4\ud55c \uc774\uc57c\uae30\uc640 \uc9c4\ub9ac\ub97c \ubc30\ubc18\ud558\ub294 \uc0ac\ub78c\ub4e4\uc758 \uba85\ub839\uc744 \uc887\uc9c0 \uc54a\uac8c \ud558\ub824 \ud568\uc774\ub77c", "content_old": "14 \uc720\ub300\uc778\uc758 \ud5c8\ud0c4\ud55c \uc774\uc57c\uae30\uc640 \uc9c4\ub9ac\ub97c \ubc30\ubc18\ud558\ub294 \uc0ac\ub78c\ub4e4\uc758 \uba85\ub839\uc744 \uc887\uc9c0 \uc54a\uac8c \ud558\ub824 \ud568\uc774\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 15, "content": "\uae68\ub057\ud55c \uc790\ub4e4\uc5d0\uac8c\ub294 \ubaa8\ub4e0 \uac83\uc774 \uae68\ub057\ud558\ub098 \ub354\ub7fd\uace0 \ubbff\uc9c0 \uc544\ub2c8\ud558\ub294 \uc790\ub4e4\uc5d0\uac8c\ub294 \uc544\ubb34 \uac83\ub3c4 \uae68\ub057\ud55c \uac83\uc774 \uc5c6\uace0 \uc624\uc9c1 \uc800\ud76c \ub9c8\uc74c\uacfc \uc591\uc2ec\uc774 \ub354\ub7ec\uc6b4\uc9c0\ub77c", "content_old": "15 \uae68\ub057\ud55c \uc790\ub4e4\uc5d0\uac8c\ub294 \ubaa8\ub4e0 \uac83\uc774 \uae68\ub057\ud558\ub098 \ub354\ub7fd\uace0 \ubbff\uc9c0 \uc544\ub2c8\ud558\ub294 \uc790\ub4e4\uc5d0\uac8c\ub294 \uc544\ubb34 \uac83\ub3c4 \uae68\ub057\ud55c \uac83\uc774 \uc5c6\uace0 \uc624\uc9c1 \uc800\ud76c \ub9c8\uc74c\uacfc \uc591\uc2ec\uc774 \ub354\ub7ec\uc6b4\uc9c0\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "1", "line": 16, "content": "\uc800\ud76c\uac00 \ud558\ub098\ub2d8\uc744 \uc2dc\uc778\ud558\ub098 \ud589\uc704\ub85c\ub294 \ubd80\uc778\ud558\ub2c8 \uac00\uc99d\ud55c \uc790\uc694 \ubcf5\uc885\uce58 \uc544\ub2c8\ud558\ub294 \uc790\uc694 \ubaa8\ub4e0 \uc120\ud55c \uc77c\uc744 \ubc84\ub9ac\ub294 \uc790\ub2c8\ub77c", "content_old": "16 \uc800\ud76c\uac00 \ud558\ub098\ub2d8\uc744 \uc2dc\uc778\ud558\ub098 \ud589\uc704\ub85c\ub294 \ubd80\uc778\ud558\ub2c8 \uac00\uc99d\ud55c \uc790\uc694 \ubcf5\uc885\uce58 \uc544\ub2c8\ud558\ub294 \uc790\uc694 \ubaa8\ub4e0 \uc120\ud55c \uc77c\uc744 \ubc84\ub9ac\ub294 \uc790\ub2c8\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 1, "content": "\uc624\uc9c1 \ub108\ub294 \ubc14\ub978 \uad50\ud6c8\uc5d0 \ud569\ud55c \uac83\uc744 \ub9d0\ud558\uc5ec", "content_old": "1 \uc624\uc9c1 \ub108\ub294 \ubc14\ub978 \uad50\ud6c8\uc5d0 \ud569\ud55c \uac83\uc744 \ub9d0\ud558\uc5ec", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 2, "content": "\ub299\uc740 \ub0a8\uc790\ub85c\ub294 \uc808\uc81c\ud558\uba70 \uacbd\uac74\ud558\uba70 \uadfc\uc2e0\ud558\uba70 \ubbff\uc74c\uacfc \uc0ac\ub791\uacfc \uc778\ub0b4\ud568\uc5d0 \uc628\uc804\ucf00 \ud558\uace0", "content_old": "2 \ub299\uc740 \ub0a8\uc790\ub85c\ub294 \uc808\uc81c\ud558\uba70 \uacbd\uac74\ud558\uba70 \uadfc\uc2e0\ud558\uba70 \ubbff\uc74c\uacfc \uc0ac\ub791\uacfc \uc778\ub0b4\ud568\uc5d0 \uc628\uc804\ucf00 \ud558\uace0", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 3, "content": "\ub299\uc740 \uc5ec\uc790\ub85c\ub294 \uc774\uc640 \uac19\uc774 \ud589\uc2e4\uc774 \uac70\ub8e9\ud558\uba70 \ucc38\uc18c\uce58 \ub9d0\uba70 \ub9ce\uc740 \uc220\uc758 \uc885\uc774 \ub418\uc9c0 \ub9d0\uba70 \uc120\ud55c \uac83\uc744 \uac00\ub974\uce58\ub294 \uc790\ub4e4\uc774 \ub418\uace0", "content_old": "3 \ub299\uc740 \uc5ec\uc790\ub85c\ub294 \uc774\uc640 \uac19\uc774 \ud589\uc2e4\uc774 \uac70\ub8e9\ud558\uba70 \ucc38\uc18c\uce58 \ub9d0\uba70 \ub9ce\uc740 \uc220\uc758 \uc885\uc774 \ub418\uc9c0 \ub9d0\uba70 \uc120\ud55c \uac83\uc744 \uac00\ub974\uce58\ub294 \uc790\ub4e4\uc774 \ub418\uace0", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 4, "content": "\uc800\ub4e4\ub85c \uc80a\uc740 \uc5ec\uc790\ub4e4\uc744 \uad50\ud6c8\ud558\ub418 \uadf8 \ub0a8\ud3b8\uacfc \uc790\ub140\ub97c \uc0ac\ub791\ud558\uba70", "content_old": "4 \uc800\ub4e4\ub85c \uc80a\uc740 \uc5ec\uc790\ub4e4\uc744 \uad50\ud6c8\ud558\ub418 \uadf8 \ub0a8\ud3b8\uacfc \uc790\ub140\ub97c \uc0ac\ub791\ud558\uba70", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 5, "content": "\uadfc\uc2e0\ud558\uba70 \uc21c\uc804\ud558\uba70 \uc9d1\uc548 \uc77c\uc744 \ud558\uba70 \uc120\ud558\uba70 \uc790\uae30 \ub0a8\ud3b8\uc5d0\uac8c \ubcf5\uc885\ud558\uac8c \ud558\ub77c \uc774\ub294 \ud558\ub098\ub2d8\uc758 \ub9d0\uc500\uc774 \ud6fc\ubc29\uc744 \ubc1b\uc9c0 \uc54a\uac8c \ud558\ub824 \ud568\uc774\ub2c8\ub77c", "content_old": "5 \uadfc\uc2e0\ud558\uba70 \uc21c\uc804\ud558\uba70 \uc9d1\uc548 \uc77c\uc744 \ud558\uba70 \uc120\ud558\uba70 \uc790\uae30 \ub0a8\ud3b8\uc5d0\uac8c \ubcf5\uc885\ud558\uac8c \ud558\ub77c \uc774\ub294 \ud558\ub098\ub2d8\uc758 \ub9d0\uc500\uc774 \ud6fc\ubc29\uc744 \ubc1b\uc9c0 \uc54a\uac8c \ud558\ub824 \ud568\uc774\ub2c8\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 6, "content": "\ub108\ub294 \uc774\uc640 \uac19\uc774 \uc80a\uc740 \ub0a8\uc790\ub4e4\uc744 \uad8c\uba74\ud558\uc5ec \uadfc\uc2e0\ud558\uac8c \ud558\ub418", "content_old": "6 \ub108\ub294 \uc774\uc640 \uac19\uc774 \uc80a\uc740 \ub0a8\uc790\ub4e4\uc744 \uad8c\uba74\ud558\uc5ec \uadfc\uc2e0\ud558\uac8c \ud558\ub418", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 7, "content": "\ubc94\uc0ac\uc5d0 \ub124 \uc790\uc2e0\uc73c\ub85c \uc120\ud55c \uc77c\uc758 \ubcf8\uc744 \ubcf4\uc5ec \uad50\ud6c8\uc758 \ubd80\ud328\uce58 \uc544\ub2c8\ud568\uacfc \uacbd\uac74\ud568\uacfc", "content_old": "7 \ubc94\uc0ac\uc5d0 \ub124 \uc790\uc2e0\uc73c\ub85c \uc120\ud55c \uc77c\uc758 \ubcf8\uc744 \ubcf4\uc5ec \uad50\ud6c8\uc758 \ubd80\ud328\uce58 \uc544\ub2c8\ud568\uacfc \uacbd\uac74\ud568\uacfc", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 8, "content": "\ucc45\ub9dd\ud560 \uac83\uc774 \uc5c6\ub294 \ubc14\ub978 \ub9d0\uc744 \ud558\uac8c \ud558\ub77c \uc774\ub294 \ub300\uc801\ud558\ub294 \uc790\ub85c \ud558\uc5ec\uae08 \ubd80\ub044\ub7ec\uc6cc \uc6b0\ub9ac\ub97c \uc545\ud558\ub2e4 \ud560 \uac83\uc774 \uc5c6\uac8c \ud558\ub824 \ud568\uc774\ub77c", "content_old": "8 \ucc45\ub9dd\ud560 \uac83\uc774 \uc5c6\ub294 \ubc14\ub978 \ub9d0\uc744 \ud558\uac8c \ud558\ub77c \uc774\ub294 \ub300\uc801\ud558\ub294 \uc790\ub85c \ud558\uc5ec\uae08 \ubd80\ub044\ub7ec\uc6cc \uc6b0\ub9ac\ub97c \uc545\ud558\ub2e4 \ud560 \uac83\uc774 \uc5c6\uac8c \ud558\ub824 \ud568\uc774\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 9, "content": "\uc885\ub4e4\ub85c\ub294 \uc790\uae30 \uc0c1\uc804\ub4e4\uc5d0\uac8c \ubc94\uc0ac\uc5d0 \uc21c\uc885\ud558\uc5ec \uae30\uc058\uac8c \ud558\uace0 \uac70\uc2a4\ub824 \ub9d0\ud558\uc9c0 \ub9d0\uba70", "content_old": "9 \uc885\ub4e4\ub85c\ub294 \uc790\uae30 \uc0c1\uc804\ub4e4\uc5d0\uac8c \ubc94\uc0ac\uc5d0 \uc21c\uc885\ud558\uc5ec \uae30\uc058\uac8c \ud558\uace0 \uac70\uc2a4\ub824 \ub9d0\ud558\uc9c0 \ub9d0\uba70", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 10, "content": "\ub5bc\uc5b4 \uba39\uc9c0 \ub9d0\uace0 \uc624\uc9c1 \uc120\ud55c \ucda9\uc131\uc744 \ub2e4\ud558\uac8c \ud558\ub77c \uc774\ub294 \ubc94\uc0ac\uc5d0 \uc6b0\ub9ac \uad6c\uc8fc \ud558\ub098\ub2d8\uc758 \uad50\ud6c8\uc744 \ube5b\ub098\uac8c \ud558\ub824 \ud568\uc774\ub77c", "content_old": "10 \ub5bc\uc5b4 \uba39\uc9c0 \ub9d0\uace0 \uc624\uc9c1 \uc120\ud55c \ucda9\uc131\uc744 \ub2e4\ud558\uac8c \ud558\ub77c \uc774\ub294 \ubc94\uc0ac\uc5d0 \uc6b0\ub9ac \uad6c\uc8fc \ud558\ub098\ub2d8\uc758 \uad50\ud6c8\uc744 \ube5b\ub098\uac8c \ud558\ub824 \ud568\uc774\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 11, "content": "\ubaa8\ub4e0 \uc0ac\ub78c\uc5d0\uac8c \uad6c\uc6d0\uc744 \uc8fc\uc2dc\ub294 \ud558\ub098\ub2d8\uc758 \uc740\ud61c\uac00 \ub098\ud0c0\ub098", "content_old": "11 \ubaa8\ub4e0 \uc0ac\ub78c\uc5d0\uac8c \uad6c\uc6d0\uc744 \uc8fc\uc2dc\ub294 \ud558\ub098\ub2d8\uc758 \uc740\ud61c\uac00 \ub098\ud0c0\ub098", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 12, "content": "\uc6b0\ub9ac\ub97c \uc591\uc721\ud558\uc2dc\ub418 \uacbd\uac74\uce58 \uc54a\uc740 \uac83\uacfc \uc774 \uc138\uc0c1 \uc815\uc695\uc744 \ub2e4 \ubc84\ub9ac\uace0 \uadfc\uc2e0\ud568\uacfc \uc758\ub85c\uc6c0\uacfc \uacbd\uac74\ud568\uc73c\ub85c \uc774 \uc138\uc0c1\uc5d0 \uc0b4\uace0", "content_old": "12 \uc6b0\ub9ac\ub97c \uc591\uc721\ud558\uc2dc\ub418 \uacbd\uac74\uce58 \uc54a\uc740 \uac83\uacfc \uc774 \uc138\uc0c1 \uc815\uc695\uc744 \ub2e4 \ubc84\ub9ac\uace0 \uadfc\uc2e0\ud568\uacfc \uc758\ub85c\uc6c0\uacfc \uacbd\uac74\ud568\uc73c\ub85c \uc774 \uc138\uc0c1\uc5d0 \uc0b4\uace0", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 13, "content": "\ubcf5\uc2a4\ub7ec\uc6b4 \uc18c\ub9dd\uacfc \uc6b0\ub9ac\uc758 \ud06c\uc2e0 \ud558\ub098\ub2d8 \uad6c\uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc601\uad11\uc774 \ub098\ud0c0\ub098\uc2ec\uc744 \uae30\ub2e4\ub9ac\uac8c \ud558\uc168\uc73c\ub2c8", "content_old": "13 \ubcf5\uc2a4\ub7ec\uc6b4 \uc18c\ub9dd\uacfc \uc6b0\ub9ac\uc758 \ud06c\uc2e0 \ud558\ub098\ub2d8 \uad6c\uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\uc758 \uc601\uad11\uc774 \ub098\ud0c0\ub098\uc2ec\uc744 \uae30\ub2e4\ub9ac\uac8c \ud558\uc168\uc73c\ub2c8", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 14, "content": "\uadf8\uac00 \uc6b0\ub9ac\ub97c \ub300\uc2e0\ud558\uc5ec \uc790\uc2e0\uc744 \uc8fc\uc2ec\uc740 \ubaa8\ub4e0 \ubd88\ubc95\uc5d0\uc11c \uc6b0\ub9ac\ub97c \uad6c\uc18d\ud558\uc2dc\uace0 \uc6b0\ub9ac\ub97c \uae68\ub057\ud558\uac8c \ud558\uc0ac \uc120\ud55c \uc77c\uc5d0 \uc5f4\uc2ec\ud558\ub294 \uce5c \ubc31\uc131\uc774 \ub418\uac8c \ud558\ub824 \ud558\uc2ec\uc774\ub2c8\ub77c", "content_old": "14 \uadf8\uac00 \uc6b0\ub9ac\ub97c \ub300\uc2e0\ud558\uc5ec \uc790\uc2e0\uc744 \uc8fc\uc2ec\uc740 \ubaa8\ub4e0 \ubd88\ubc95\uc5d0\uc11c \uc6b0\ub9ac\ub97c \uad6c\uc18d\ud558\uc2dc\uace0 \uc6b0\ub9ac\ub97c \uae68\ub057\ud558\uac8c \ud558\uc0ac \uc120\ud55c \uc77c\uc5d0 \uc5f4\uc2ec\ud558\ub294 \uce5c \ubc31\uc131\uc774 \ub418\uac8c \ud558\ub824 \ud558\uc2ec\uc774\ub2c8\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "2", "line": 15, "content": "\ub108\ub294 \uc774\uac83\uc744 \ub9d0\ud558\uace0 \uad8c\uba74\ud558\uba70 \ubaa8\ub4e0 \uad8c\uc704\ub85c \ucc45\ub9dd\ud558\uc5ec \ub204\uad6c\uc5d0\uac8c\ub4e0\uc9c0 \uc5c5\uc2e0\uc5ec\uae40\uc744 \ubc1b\uc9c0 \ub9d0\ub77c", "content_old": "15 \ub108\ub294 \uc774\uac83\uc744 \ub9d0\ud558\uace0 \uad8c\uba74\ud558\uba70 \ubaa8\ub4e0 \uad8c\uc704\ub85c \ucc45\ub9dd\ud558\uc5ec \ub204\uad6c\uc5d0\uac8c\ub4e0\uc9c0 \uc5c5\uc2e0\uc5ec\uae40\uc744 \ubc1b\uc9c0 \ub9d0\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 1, "content": "\ub108\ub294 \uc800\ud76c\ub85c \ud558\uc5ec\uae08 \uc815\uc0ac\uc640 \uad8c\uc138 \uc7a1\uc740 \uc790\ub4e4\uc5d0\uac8c \ubcf5\uc885\ud558\uba70 \uc21c\uc885\ud558\uba70 \ubaa8\ub4e0 \uc120\ud55c \uc77c \ud589\ud558\uae30\ub97c \uc608\ube44\ud558\uac8c \ud558\uba70", "content_old": "1 \ub108\ub294 \uc800\ud76c\ub85c \ud558\uc5ec\uae08 \uc815\uc0ac\uc640 \uad8c\uc138 \uc7a1\uc740 \uc790\ub4e4\uc5d0\uac8c \ubcf5\uc885\ud558\uba70 \uc21c\uc885\ud558\uba70 \ubaa8\ub4e0 \uc120\ud55c \uc77c \ud589\ud558\uae30\ub97c \uc608\ube44\ud558\uac8c \ud558\uba70", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 2, "content": "\uc544\ubb34\ub3c4 \ud6fc\ubc29\ud558\uc9c0 \ub9d0\uba70 \ub2e4\ud22c\uc9c0 \ub9d0\uba70 \uad00\uc6a9\ud558\uba70 \ubc94\uc0ac\uc5d0 \uc628\uc720\ud568\uc744 \ubaa8\ub4e0 \uc0ac\ub78c\uc5d0\uac8c \ub098\ud0c0\ub0bc \uac83\uc744 \uae30\uc5b5\ud558\uac8c \ud558\ub77c", "content_old": "2 \uc544\ubb34\ub3c4 \ud6fc\ubc29\ud558\uc9c0 \ub9d0\uba70 \ub2e4\ud22c\uc9c0 \ub9d0\uba70 \uad00\uc6a9\ud558\uba70 \ubc94\uc0ac\uc5d0 \uc628\uc720\ud568\uc744 \ubaa8\ub4e0 \uc0ac\ub78c\uc5d0\uac8c \ub098\ud0c0\ub0bc \uac83\uc744 \uae30\uc5b5\ud558\uac8c \ud558\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 3, "content": "\uc6b0\ub9ac\ub3c4 \uc804\uc5d0\ub294 \uc5b4\ub9ac\uc11d\uc740 \uc790\uc694 \uc21c\uc885\uce58 \uc544\ub2c8\ud55c \uc790\uc694 \uc18d\uc740 \uc790\uc694 \uac01\uc0c9 \uc815\uc695\uacfc \ud589\ub77d\uc5d0 \uc885\ub178\ub987\ud55c \uc790\uc694 \uc545\ub3c5\uacfc \ud22c\uae30\ub85c \uc9c0\ub0b8 \uc790\uc694 \uac00\uc99d\uc2a4\ub7ec\uc6b4 \uc790\uc694 \ud53c\ucc28 \ubbf8\uc6cc\ud55c \uc790\uc774\uc5c8\uc73c\ub098", "content_old": "3 \uc6b0\ub9ac\ub3c4 \uc804\uc5d0\ub294 \uc5b4\ub9ac\uc11d\uc740 \uc790\uc694 \uc21c\uc885\uce58 \uc544\ub2c8\ud55c \uc790\uc694 \uc18d\uc740 \uc790\uc694 \uac01\uc0c9 \uc815\uc695\uacfc \ud589\ub77d\uc5d0 \uc885\ub178\ub987\ud55c \uc790\uc694 \uc545\ub3c5\uacfc \ud22c\uae30\ub85c \uc9c0\ub0b8 \uc790\uc694 \uac00\uc99d\uc2a4\ub7ec\uc6b4 \uc790\uc694 \ud53c\ucc28 \ubbf8\uc6cc\ud55c \uc790\uc774\uc5c8\uc73c\ub098", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 4, "content": "\uc6b0\ub9ac \uad6c\uc8fc \ud558\ub098\ub2d8\uc758 \uc790\ube44\uc640 \uc0ac\ub78c \uc0ac\ub791\ud558\uc2ec\uc744 \ub098\ud0c0\ub0b4\uc2e4 \ub54c\uc5d0", "content_old": "4 \uc6b0\ub9ac \uad6c\uc8fc \ud558\ub098\ub2d8\uc758 \uc790\ube44\uc640 \uc0ac\ub78c \uc0ac\ub791\ud558\uc2ec\uc744 \ub098\ud0c0\ub0b4\uc2e4 \ub54c\uc5d0", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 5, "content": "\uc6b0\ub9ac\ub97c \uad6c\uc6d0\ud558\uc2dc\ub418 \uc6b0\ub9ac\uc758 \ud589\ud55c\ubc14 \uc758\ub85c\uc6b4 \ud589\uc704\ub85c \ub9d0\ubbf8\uc554\uc9c0 \uc544\ub2c8\ud558\uace0 \uc624\uc9c1 \uadf8\uc758 \uae0d\ud73c\ud558\uc2ec\uc744 \uc887\uc544 \uc911\uc0dd\uc758 \uc53b\uc74c\uacfc \uc131\ub839\uc758 \uc0c8\ub86d\uac8c \ud558\uc2ec\uc73c\ub85c \ud558\uc168\ub098\ub2c8", "content_old": "5 \uc6b0\ub9ac\ub97c \uad6c\uc6d0\ud558\uc2dc\ub418 \uc6b0\ub9ac\uc758 \ud589\ud55c\ubc14 \uc758\ub85c\uc6b4 \ud589\uc704\ub85c \ub9d0\ubbf8\uc554\uc9c0 \uc544\ub2c8\ud558\uace0 \uc624\uc9c1 \uadf8\uc758 \uae0d\ud73c\ud558\uc2ec\uc744 \uc887\uc544 \uc911\uc0dd\uc758 \uc53b\uc74c\uacfc \uc131\ub839\uc758 \uc0c8\ub86d\uac8c \ud558\uc2ec\uc73c\ub85c \ud558\uc168\ub098\ub2c8", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 6, "content": "\uc131\ub839\uc744 \uc6b0\ub9ac \uad6c\uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\ub85c \ub9d0\ubbf8\uc554\uc544 \uc6b0\ub9ac\uc5d0\uac8c \ud48d\uc131\ud788 \ubd80\uc5b4\uc8fc\uc0ac", "content_old": "6 \uc131\ub839\uc744 \uc6b0\ub9ac \uad6c\uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\ub85c \ub9d0\ubbf8\uc554\uc544 \uc6b0\ub9ac\uc5d0\uac8c \ud48d\uc131\ud788 \ubd80\uc5b4\uc8fc\uc0ac", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 7, "content": "\uc6b0\ub9ac\ub85c \uc800\uc758 \uc740\ud61c\ub97c \ud798\uc785\uc5b4 \uc758\ub86d\ub2e4 \ud558\uc2ec\uc744 \uc5bb\uc5b4 \uc601\uc0dd\uc758 \uc18c\ub9dd\uc744 \ub530\ub77c \ud6c4\uc0ac\uac00 \ub418\uac8c \ud558\ub824 \ud558\uc2ec\uc774\ub77c", "content_old": "7 \uc6b0\ub9ac\ub85c \uc800\uc758 \uc740\ud61c\ub97c \ud798\uc785\uc5b4 \uc758\ub86d\ub2e4 \ud558\uc2ec\uc744 \uc5bb\uc5b4 \uc601\uc0dd\uc758 \uc18c\ub9dd\uc744 \ub530\ub77c \ud6c4\uc0ac\uac00 \ub418\uac8c \ud558\ub824 \ud558\uc2ec\uc774\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 8, "content": "\uc774 \ub9d0\uc774 \ubbf8\uc058\ub3c4\ub2e4 \uc6d0\ucee8\ub300 \ub124\uac00 \uc774 \uc5ec\ub7ec \uac83\uc5d0 \ub300\ud558\uc5ec \uad73\uc138\uac8c \ub9d0\ud558\ub77c \uc774\ub294 \ud558\ub098\ub2d8\uc744 \ubbff\ub294 \uc790\ub4e4\ub85c \ud558\uc5ec\uae08 \uc870\uc2ec\ud558\uc5ec \uc120\ud55c \uc77c\uc744 \ud798\uc4f0\uac8c \ud558\ub824 \ud568\uc774\ub77c \uc774\uac83\uc740 \uc544\ub984\ub2e4\uc6b0\uba70 \uc0ac\ub78c\ub4e4\uc5d0\uac8c \uc720\uc775\ud558\ub2c8\ub77c", "content_old": "8 \uc774 \ub9d0\uc774 \ubbf8\uc058\ub3c4\ub2e4 \uc6d0\ucee8\ub300 \ub124\uac00 \uc774 \uc5ec\ub7ec \uac83\uc5d0 \ub300\ud558\uc5ec \uad73\uc138\uac8c \ub9d0\ud558\ub77c \uc774\ub294 \ud558\ub098\ub2d8\uc744 \ubbff\ub294 \uc790\ub4e4\ub85c \ud558\uc5ec\uae08 \uc870\uc2ec\ud558\uc5ec \uc120\ud55c \uc77c\uc744 \ud798\uc4f0\uac8c \ud558\ub824 \ud568\uc774\ub77c \uc774\uac83\uc740 \uc544\ub984\ub2e4\uc6b0\uba70 \uc0ac\ub78c\ub4e4\uc5d0\uac8c \uc720\uc775\ud558\ub2c8\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 9, "content": "\uadf8\ub7ec\ub098 \uc5b4\ub9ac\uc11d\uc740 \ubcc0\ub860\uacfc \uc871\ubcf4 \uc774\uc57c\uae30\uc640 \ubd84\uc7c1\uacfc \uc728\ubc95\uc5d0 \ub300\ud55c \ub2e4\ud23c\uc744 \ud53c\ud558\ub77c \uc774\uac83\uc740 \ubb34\uc775\ud55c \uac83\uc774\uc694 \ud5db\ub41c \uac83\uc774\ub2c8\ub77c", "content_old": "9 \uadf8\ub7ec\ub098 \uc5b4\ub9ac\uc11d\uc740 \ubcc0\ub860\uacfc \uc871\ubcf4 \uc774\uc57c\uae30\uc640 \ubd84\uc7c1\uacfc \uc728\ubc95\uc5d0 \ub300\ud55c \ub2e4\ud23c\uc744 \ud53c\ud558\ub77c \uc774\uac83\uc740 \ubb34\uc775\ud55c \uac83\uc774\uc694 \ud5db\ub41c \uac83\uc774\ub2c8\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 10, "content": "\uc774\ub2e8\uc5d0 \uc18d\ud55c \uc0ac\ub78c\uc744 \ud55c \ub450\ubc88 \ud6c8\uacc4\ud55c \ud6c4\uc5d0 \uba40\ub9ac \ud558\ub77c", "content_old": "10 \uc774\ub2e8\uc5d0 \uc18d\ud55c \uc0ac\ub78c\uc744 \ud55c \ub450\ubc88 \ud6c8\uacc4\ud55c \ud6c4\uc5d0 \uba40\ub9ac \ud558\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 11, "content": "\uc774\ub7ec\ud55c \uc0ac\ub78c\uc740 \ub124\uac00 \uc544\ub294 \ubc14\uc640 \uac19\uc774 \ubd80\ud328\ud558\uc5ec\uc11c \uc2a4\uc2a4\ub85c \uc815\uc8c4\ud55c \uc790\ub85c\uc11c \uc8c4\ub97c \uc9d3\ub290\ub2c8\ub77c", "content_old": "11 \uc774\ub7ec\ud55c \uc0ac\ub78c\uc740 \ub124\uac00 \uc544\ub294 \ubc14\uc640 \uac19\uc774 \ubd80\ud328\ud558\uc5ec\uc11c \uc2a4\uc2a4\ub85c \uc815\uc8c4\ud55c \uc790\ub85c\uc11c \uc8c4\ub97c \uc9d3\ub290\ub2c8\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 12, "content": "\ub0b4\uac00 \uc544\ub370\ub9c8\ub098 \ub450\uae30\uace0\ub97c \ub124\uac8c \ubcf4\ub0b4\ub9ac\ub2c8 \uadf8 \ub54c\uc5d0 \ub124\uac00 \uae09\ud788 \ub2c8\uace0\ubcfc\ub9ac\ub85c \ub0b4\uac8c \uc624\ub77c \ub0b4\uac00 \uac70\uae30\uc11c \uacfc\ub3d9\ud558\uae30\ub85c \uc791\uc815\ud558\uc600\ub178\ub77c", "content_old": "12 \ub0b4\uac00 \uc544\ub370\ub9c8\ub098 \ub450\uae30\uace0\ub97c \ub124\uac8c \ubcf4\ub0b4\ub9ac\ub2c8 \uadf8 \ub54c\uc5d0 \ub124\uac00 \uae09\ud788 \ub2c8\uace0\ubcfc\ub9ac\ub85c \ub0b4\uac8c \uc624\ub77c \ub0b4\uac00 \uac70\uae30\uc11c \uacfc\ub3d9\ud558\uae30\ub85c \uc791\uc815\ud558\uc600\ub178\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 13, "content": "\uad50\ubc95\uc0ac \uc138\ub098\uc640 \ubc0f \uc544\ubcfc\ub85c\ub97c \uae09\ud788 \uba3c\uc800 \ubcf4\ub0b4\uc5b4 \uc800\ud76c\ub85c \uad81\ud54d\ud568\uc774 \uc5c6\uac8c \ud558\uace0", "content_old": "13 \uad50\ubc95\uc0ac \uc138\ub098\uc640 \ubc0f \uc544\ubcfc\ub85c\ub97c \uae09\ud788 \uba3c\uc800 \ubcf4\ub0b4\uc5b4 \uc800\ud76c\ub85c \uad81\ud54d\ud568\uc774 \uc5c6\uac8c \ud558\uace0", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 14, "content": "\ub610 \uc6b0\ub9ac \uc0ac\ub78c\ub4e4\ub3c4 \uc5f4\ub9e4 \uc5c6\ub294 \uc790\uac00 \ub418\uc9c0 \uc54a\uac8c \ud558\uae30 \uc704\ud558\uc5ec \ud544\uc694\ud55c \uac83\uc744 \uc608\ube44\ud558\ub294 \uc88b\uc740 \uc77c\uc5d0 \ud798\uc4f0\uae30\ub97c \ubc30\uc6b0\uac8c \ud558\ub77c", "content_old": "14 \ub610 \uc6b0\ub9ac \uc0ac\ub78c\ub4e4\ub3c4 \uc5f4\ub9e4 \uc5c6\ub294 \uc790\uac00 \ub418\uc9c0 \uc54a\uac8c \ud558\uae30 \uc704\ud558\uc5ec \ud544\uc694\ud55c \uac83\uc744 \uc608\ube44\ud558\ub294 \uc88b\uc740 \uc77c\uc5d0 \ud798\uc4f0\uae30\ub97c \ubc30\uc6b0\uac8c \ud558\ub77c", "checked": 1}, {"bible_type": 1, "lang": "ko", "chapter": "3", "line": 15, "content": "\ub098\uc640 \ud568\uaed8 \uc788\ub294 \uc790\uac00 \ub2e4 \ub124\uac8c \ubb38\uc548\ud558\ub2c8 \ubbff\uc74c \uc548\uc5d0\uc11c \uc6b0\ub9ac\ub97c \uc0ac\ub791\ud558\ub294 \uc790\ub4e4\uc5d0\uac8c \ub108\ub3c4 \ubb38\uc548\ud558\ub77c \uc740\ud61c\uac00 \ub108\ud76c \ubb34\ub9ac\uc5d0\uac8c \uc788\uc744\uc9c0\uc5b4\ub2e4", "content_old": "15 \ub098\uc640 \ud568\uaed8 \uc788\ub294 \uc790\uac00 \ub2e4 \ub124\uac8c \ubb38\uc548\ud558\ub2c8 \ubbff\uc74c \uc548\uc5d0\uc11c \uc6b0\ub9ac\ub97c \uc0ac\ub791\ud558\ub294 \uc790\ub4e4\uc5d0\uac8c \ub108\ub3c4 \ubb38\uc548\ud558\ub77c \uc740\ud61c\uac00 \ub108\ud76c \ubb34\ub9ac\uc5d0\uac8c \uc788\uc744\uc9c0\uc5b4\ub2e4", "checked": 1}]