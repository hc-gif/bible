[{"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 1, "content": "\uc544\ubaac\uc758 \uc544\ub4e4 \uc720\ub2e4 \uc655 \uc694\uc2dc\uc544\uc758 \uc2dc\ub300\uc5d0 \uc2a4\ubc14\ub0d0\uc5d0\uac8c \uc784\ud55c \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774\ub77c \uc2a4\ubc14\ub0d0\ub294 \ud788\uc2a4\uae30\uc57c\uc758 \ud604\uc190\uc774\uc694 \uc544\ub9c8\ub7b4\uc758 \uc99d\uc190\uc774\uc694 \uadf8\ub2e4\ub7b4\uc758 \uc190\uc790\uc694 \uad6c\uc2dc\uc758 \uc544\ub4e4\uc774\uc5c8\ub354\ub77c", "content_old": "1 \uc544\ubaac\uc758 \uc544\ub4e4 \uc720\ub2e4 \uc655 \uc694\uc2dc\uc544\uc758 \uc2dc\ub300\uc5d0 \uc2a4\ubc14\ub0d0\uc5d0\uac8c \uc784\ud55c \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774\ub77c \uc2a4\ubc14\ub0d0\ub294 \ud788\uc2a4\uae30\uc57c\uc758 \ud604\uc190\uc774\uc694 \uc544\ub9c8\ub7b4\uc758 \uc99d\uc190\uc774\uc694 \uadf8\ub2e4\ub7b4\uc758 \uc190\uc790\uc694 \uad6c\uc2dc\uc758 \uc544\ub4e4\uc774\uc5c8\ub354\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 2, "content": "\uc5ec\ud638\uc640\uaed8\uc11c \uac00\ub77c\uc0ac\ub300 \ub0b4\uac00 \uc9c0\uba74\uc5d0\uc11c \ubaa8\ub4e0 \uac83\uc744 \uc9c4\uba78\ud558\ub9ac\ub77c", "content_old": "2 \uc5ec\ud638\uc640\uaed8\uc11c \uac00\ub77c\uc0ac\ub300 \ub0b4\uac00 \uc9c0\uba74\uc5d0\uc11c \ubaa8\ub4e0 \uac83\uc744 \uc9c4\uba78\ud558\ub9ac\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 3, "content": "\ub0b4\uac00 \uc0ac\ub78c\uacfc \uc9d0\uc2b9\uc744 \uc9c4\uba78\ud558\uace0 \uacf5\uc911\uc758 \uc0c8\uc640 \ubc14\ub2e4\uc758 \uace0\uae30\uc640 \uac70\uce58\uac8c \ud558\ub294\uac83\uacfc \uc545\uc778\ub4e4\uc744 \uc544\uc6b8\ub7ec \uc9c4\uba78\ud560 \uac83\uc774\ub77c \ub0b4\uac00 \uc0ac\ub78c\uc744 \uc9c0\uba74\uc5d0\uc11c \uba78\uc808\ud558\ub9ac\ub77c \ub098 \uc5ec\ud638\uc640\uc758 \ub9d0\uc774\ub2c8\ub77c", "content_old": "3 \ub0b4\uac00 \uc0ac\ub78c\uacfc \uc9d0\uc2b9\uc744 \uc9c4\uba78\ud558\uace0 \uacf5\uc911\uc758 \uc0c8\uc640 \ubc14\ub2e4\uc758 \uace0\uae30\uc640 \uac70\uce58\uac8c \ud558\ub294\uac83\uacfc \uc545\uc778\ub4e4\uc744 \uc544\uc6b8\ub7ec \uc9c4\uba78\ud560 \uac83\uc774\ub77c \ub0b4\uac00 \uc0ac\ub78c\uc744 \uc9c0\uba74\uc5d0\uc11c \uba78\uc808\ud558\ub9ac\ub77c \ub098 \uc5ec\ud638\uc640\uc758 \ub9d0\uc774\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 4, "content": "\ub0b4\uac00 \uc720\ub2e4\uc640 \uc608\ub8e8\uc0b4\ub818 \ubaa8\ub4e0 \uac70\ubbfc \uc704\uc5d0 \uc190\uc744 \ud3b4\uc11c \ubc14\uc54c\uc758 \ub0a8\uc544 \uc788\ub294 \uac83\uc744 \uadf8\uacf3\uc5d0\uc11c \uba78\uc808\ud558\uba70 \uadf8\ub9c8\ub9bc\uc774\ub780 \uc774\ub984\uacfc \ubc0f \uadf8 \uc81c\uc0ac\uc7a5\ub4e4\uc744 \uc544\uc6b8\ub7ec \uba78\uc808\ud558\uba70", "content_old": "4 \ub0b4\uac00 \uc720\ub2e4\uc640 \uc608\ub8e8\uc0b4\ub818 \ubaa8\ub4e0 \uac70\ubbfc \uc704\uc5d0 \uc190\uc744 \ud3b4\uc11c \ubc14\uc54c\uc758 \ub0a8\uc544 \uc788\ub294 \uac83\uc744 \uadf8\uacf3\uc5d0\uc11c \uba78\uc808\ud558\uba70 \uadf8\ub9c8\ub9bc\uc774\ub780 \uc774\ub984\uacfc \ubc0f \uadf8 \uc81c\uc0ac\uc7a5\ub4e4\uc744 \uc544\uc6b8\ub7ec \uba78\uc808\ud558\uba70", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 5, "content": "\ubb34\ub987 \uc9c0\ubd95\uc5d0\uc11c \ud558\ub298\uc758 \uc77c\uc6d4\uc131\uc2e0\uc5d0\uac8c \uacbd\ubc30\ud558\uba70 \uc5ec\ud638\uc640\uaed8 \ub9f9\uc138\ud558\uba74\uc11c \ub9d0\uac10\uc744 \uac00\ub9ac\ucf1c \ub9f9\uc138\ud558\ub294 \uc790\uc640", "content_old": "5 \ubb34\ub987 \uc9c0\ubd95\uc5d0\uc11c \ud558\ub298\uc758 \uc77c\uc6d4\uc131\uc2e0\uc5d0\uac8c \uacbd\ubc30\ud558\uba70 \uc5ec\ud638\uc640\uaed8 \ub9f9\uc138\ud558\uba74\uc11c \ub9d0\uac10\uc744 \uac00\ub9ac\ucf1c \ub9f9\uc138\ud558\ub294 \uc790\uc640", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 6, "content": "\uc5ec\ud638\uc640\ub97c \ubc30\ubc18\ud558\uace0 \uc887\uc9c0 \uc544\ub2c8\ud55c \uc790\uc640 \uc5ec\ud638\uc640\ub97c \ucc3e\uc9c0\ub3c4 \uc544\ub2c8\ud558\uba70 \uad6c\ud558\uc9c0\ub3c4 \uc544\ub2c8\ud55c \uc790\ub97c \uba78\uc808\ud558\ub9ac\ub77c", "content_old": "6 \uc5ec\ud638\uc640\ub97c \ubc30\ubc18\ud558\uace0 \uc887\uc9c0 \uc544\ub2c8\ud55c \uc790\uc640 \uc5ec\ud638\uc640\ub97c \ucc3e\uc9c0\ub3c4 \uc544\ub2c8\ud558\uba70 \uad6c\ud558\uc9c0\ub3c4 \uc544\ub2c8\ud55c \uc790\ub97c \uba78\uc808\ud558\ub9ac\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 7, "content": "\uc8fc \uc5ec\ud638\uc640 \uc55e\uc5d0\uc11c \uc7a0\uc7a0\ud560\uc9c0\uc5b4\ub2e4 \uc774\ub294 \uc5ec\ud638\uc640\uc758 \ub0a0\uc774 \uac00\uae4c\uc654\uc73c\ubbc0 \ub85c \uc5ec\ud638\uc640\uac00 \ud76c\uc0dd\uc744 \uc900\ube44\ud558\uace0 \uadf8 \uccad\ud560 \uc790\ub97c \uad6c\ubcc4\ud558\uc600\uc74c\uc774\ub2c8\ub77c", "content_old": "7 \uc8fc \uc5ec\ud638\uc640 \uc55e\uc5d0\uc11c \uc7a0\uc7a0\ud560\uc9c0\uc5b4\ub2e4 \uc774\ub294 \uc5ec\ud638\uc640\uc758 \ub0a0\uc774 \uac00\uae4c\uc654\uc73c\ubbc0 \ub85c \uc5ec\ud638\uc640\uac00 \ud76c\uc0dd\uc744 \uc900\ube44\ud558\uace0 \uadf8 \uccad\ud560 \uc790\ub97c \uad6c\ubcc4\ud558\uc600\uc74c\uc774\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 8, "content": "\uc5ec\ud638\uc640\uc758 \ud76c\uc0dd\uc758 \ub0a0\uc5d0 \ub0b4\uac00 \ubc29\ubc31\ub4e4\uacfc \uc655\uc790\ub4e4\uacfc \uc774\ubc29\uc758 \uc758\ubcf5\uc744 \uc785\uc740 \uc790\ub4e4\uc744 \ubc8c\ud560 \uac83\uc774\uba70", "content_old": "8 \uc5ec\ud638\uc640\uc758 \ud76c\uc0dd\uc758 \ub0a0\uc5d0 \ub0b4\uac00 \ubc29\ubc31\ub4e4\uacfc \uc655\uc790\ub4e4\uacfc \uc774\ubc29\uc758 \uc758\ubcf5\uc744 \uc785\uc740 \uc790\ub4e4\uc744 \ubc8c\ud560 \uac83\uc774\uba70", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 9, "content": "\uadf8 \ub0a0\uc5d0 \ubb38\ud131\uc744 \ub6f0\uc5b4 \ub118\uc5b4\uc11c \uac15\ud3ec\uc640 \uada4\ud73c\ub85c \uc790\uae30 \uc8fc\uc778\uc758 \uc9d1\uc5d0 \ucc44\uc6b4 \uc790\ub4e4\uc744 \ub0b4\uac00 \ubc8c\ud558\ub9ac\ub77c", "content_old": "9 \uadf8 \ub0a0\uc5d0 \ubb38\ud131\uc744 \ub6f0\uc5b4 \ub118\uc5b4\uc11c \uac15\ud3ec\uc640 \uada4\ud73c\ub85c \uc790\uae30 \uc8fc\uc778\uc758 \uc9d1\uc5d0 \ucc44\uc6b4 \uc790\ub4e4\uc744 \ub0b4\uac00 \ubc8c\ud558\ub9ac\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 10, "content": "\"\ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\ub178\ub77c \uadf8\ub0a0\uc5d0 \uc5b4\ubb38\uc5d0\uc11c\ub294 \uace1\uc131\uc774, \uc81c \uc774 \uad6c\uc5ed\uc5d0 \uc11c\ub294 \ubd80\ub974\uc9d6\uc740 \uc18c\ub9ac\uac00 \uc77c\uc5b4\ub098\ub9ac\ub77c\"", "content_old": "10 \"\ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\ub178\ub77c \uadf8\ub0a0\uc5d0 \uc5b4\ubb38\uc5d0\uc11c\ub294 \uace1\uc131\uc774, \uc81c \uc774 \uad6c\uc5ed\uc5d0 \uc11c\ub294 \ubd80\ub974\uc9d6\uc740 \uc18c\ub9ac\uac00 \uc77c\uc5b4\ub098\ub9ac\ub77c\"", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 11, "content": "\ub9c9\ub370\uc2a4 \uac70\ubbfc\ub4e4\uc544 \ub108\ud76c\ub294 \uc560\uace1\ud558\ub77c \uac00\ub098\uc548 \ubc31\uc131\uc774 \ub2e4 \ud328\ub9dd\ud558\uace0 \uc740\uc744 \uc218\uc6b4\ud558\ub294 \uc790\uac00 \ub04a\uc5b4\uc84c\uc74c\uc774\ub2c8\ub77c", "content_old": "11 \ub9c9\ub370\uc2a4 \uac70\ubbfc\ub4e4\uc544 \ub108\ud76c\ub294 \uc560\uace1\ud558\ub77c \uac00\ub098\uc548 \ubc31\uc131\uc774 \ub2e4 \ud328\ub9dd\ud558\uace0 \uc740\uc744 \uc218\uc6b4\ud558\ub294 \uc790\uac00 \ub04a\uc5b4\uc84c\uc74c\uc774\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 12, "content": "\uadf8\ub54c\uc5d0 \ub0b4\uac00 \ub4f1\ubd88\ub85c \uc608\ub8e8\uc0b4\ub818\uc5d0 \ub450\ub8e8 \ucc3e\uc544 \ubb34\ub987 \ucc0c\ub07c\uac19\uc774 \uac00\ub77c\uc549\uc544\uc11c \uc2ec\uc911\uc5d0 \uc2a4\uc2a4\ub85c \uc774\ub974\uae30\ub97c \uc5ec\ud638\uc640\uaed8\uc11c\ub294 \ubcf5\ub3c4 \ub0b4\ub9ac\uc9c0 \uc544\ub2c8\ud558\uc2dc\uba70 \ud654\ub3c4 \ub0b4\ub9ac\uc9c0 \uc544\ub2c8\ud558\uc2dc\ub9ac\ub77c \ud558\ub294 \uc790\ub97c \ubc8c\ud558\ub9ac\ub2c8", "content_old": "12 \uadf8\ub54c\uc5d0 \ub0b4\uac00 \ub4f1\ubd88\ub85c \uc608\ub8e8\uc0b4\ub818\uc5d0 \ub450\ub8e8 \ucc3e\uc544 \ubb34\ub987 \ucc0c\ub07c\uac19\uc774 \uac00\ub77c\uc549\uc544\uc11c \uc2ec\uc911\uc5d0 \uc2a4\uc2a4\ub85c \uc774\ub974\uae30\ub97c \uc5ec\ud638\uc640\uaed8\uc11c\ub294 \ubcf5\ub3c4 \ub0b4\ub9ac\uc9c0 \uc544\ub2c8\ud558\uc2dc\uba70 \ud654\ub3c4 \ub0b4\ub9ac\uc9c0 \uc544\ub2c8\ud558\uc2dc\ub9ac\ub77c \ud558\ub294 \uc790\ub97c \ubc8c\ud558\ub9ac\ub2c8", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 13, "content": "\uadf8\ub4e4\uc758 \uc7ac\ubb3c\uc774 \ub178\ub7b5\ub418\uba70 \uadf8\ub4e4\uc758 \uc9d1\uc774 \ud669\ubb34\ud560 \uac83\uc774\ub77c \uadf8\ub4e4\uc774 \uc9d1\uc744 \uac74\ucd95\ud558\ub098 \uac70\uae30 \uac70\ud558\uc9c0 \ubabb\ud558\uba70 \ud3ec\ub3c4\uc6d0\uc744 \uc2ec\uc73c\ub098 \uadf8 \ud3ec\ub3c4\uc8fc\ub97c \ub9c8\uc2dc\uc9c0 \ubabb\ud558\ub9ac\ub77c", "content_old": "13 \uadf8\ub4e4\uc758 \uc7ac\ubb3c\uc774 \ub178\ub7b5\ub418\uba70 \uadf8\ub4e4\uc758 \uc9d1\uc774 \ud669\ubb34\ud560 \uac83\uc774\ub77c \uadf8\ub4e4\uc774 \uc9d1\uc744 \uac74\ucd95\ud558\ub098 \uac70\uae30 \uac70\ud558\uc9c0 \ubabb\ud558\uba70 \ud3ec\ub3c4\uc6d0\uc744 \uc2ec\uc73c\ub098 \uadf8 \ud3ec\ub3c4\uc8fc\ub97c \ub9c8\uc2dc\uc9c0 \ubabb\ud558\ub9ac\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 14, "content": "\uc5ec\ud638\uc640\uc758 \ud070 \ub0a0\uc774 \uac00\uae5d\ub3c4\ub2e4 \uac00\uae5d\uace0\ub3c4 \uc2ec\ud788 \ube60\ub974\ub3c4\ub2e4 \uc5ec\ud638\uc640\uc758 \ub0a0\uc758 \uc18c\ub9ac\ub85c\ub2e4 \uc6a9\uc0ac\uac00 \uac70\uae30\uc11c \uc2ec\ud788 \uc560\uace1\ud558\ub294\ub3c4\ub2e4", "content_old": "14 \uc5ec\ud638\uc640\uc758 \ud070 \ub0a0\uc774 \uac00\uae5d\ub3c4\ub2e4 \uac00\uae5d\uace0\ub3c4 \uc2ec\ud788 \ube60\ub974\ub3c4\ub2e4 \uc5ec\ud638\uc640\uc758 \ub0a0\uc758 \uc18c\ub9ac\ub85c\ub2e4 \uc6a9\uc0ac\uac00 \uac70\uae30\uc11c \uc2ec\ud788 \uc560\uace1\ud558\ub294\ub3c4\ub2e4", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 15, "content": "\uadf8\ub0a0\uc740 \ubd84\ub178\uc758 \ub0a0\uc774\uc694 \ud658\ub09c\uacfc \uace0\ud1b5\uc758 \ub0a0\uc774\uc694 \ud669\ubb34\uc640 \ud328\uad34\uc758 \ub0a0\uc774\uc694 \uce84\uce84\ud558\uace0 \uc5b4\ub450\uc6b4 \ub0a0\uc774\uc694 \uad6c\ub984\uacfc \ud751\uc554\uc758 \ub0a0\uc774\uc694", "content_old": "15 \uadf8\ub0a0\uc740 \ubd84\ub178\uc758 \ub0a0\uc774\uc694 \ud658\ub09c\uacfc \uace0\ud1b5\uc758 \ub0a0\uc774\uc694 \ud669\ubb34\uc640 \ud328\uad34\uc758 \ub0a0\uc774\uc694 \uce84\uce84\ud558\uace0 \uc5b4\ub450\uc6b4 \ub0a0\uc774\uc694 \uad6c\ub984\uacfc \ud751\uc554\uc758 \ub0a0\uc774\uc694", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 16, "content": "\ub098\ud314\uc744 \ubd88\uc5b4 \uacbd\uace0\ud558\uba70 \uacac\uace0\ud55c \uc131\uc74d\uc744 \uce58\uba70 \ub192\uc740 \ub9dd\ub300\ub97c \uce58\ub294 \ub0a0\uc774\ub85c\ub2e4", "content_old": "16 \ub098\ud314\uc744 \ubd88\uc5b4 \uacbd\uace0\ud558\uba70 \uacac\uace0\ud55c \uc131\uc74d\uc744 \uce58\uba70 \ub192\uc740 \ub9dd\ub300\ub97c \uce58\ub294 \ub0a0\uc774\ub85c\ub2e4", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 17, "content": "\ub0b4\uac00 \uc0ac\ub78c\ub4e4\uc5d0\uac8c \uace0\ub09c\uc744 \ub0b4\ub824 \uc18c\uacbd\uac19\uc774 \ud589\ud558\uac8c \ud558\ub9ac\ub2c8 \uc774\ub294 \uadf8\ub4e4\uc774 \ub098 \uc5ec\ud638\uc640\uaed8 \ubc94\uc8c4\ud558\uc600\uc74c\uc774\ub77c \ub610 \uadf8\ub4e4\uc758 \ud53c\ub294 \ud758\ub9ac\uc6cc\uc11c \ud2f0\ub04c\uac19\uc774 \ub418\uba70 \uadf8\ub4e4\uc758 \uc0b4\uc740 \ubd84\ud1a0\uac19\uc774 \ub420\uc9c0\ub77c", "content_old": "17 \ub0b4\uac00 \uc0ac\ub78c\ub4e4\uc5d0\uac8c \uace0\ub09c\uc744 \ub0b4\ub824 \uc18c\uacbd\uac19\uc774 \ud589\ud558\uac8c \ud558\ub9ac\ub2c8 \uc774\ub294 \uadf8\ub4e4\uc774 \ub098 \uc5ec\ud638\uc640\uaed8 \ubc94\uc8c4\ud558\uc600\uc74c\uc774\ub77c \ub610 \uadf8\ub4e4\uc758 \ud53c\ub294 \ud758\ub9ac\uc6cc\uc11c \ud2f0\ub04c\uac19\uc774 \ub418\uba70 \uadf8\ub4e4\uc758 \uc0b4\uc740 \ubd84\ud1a0\uac19\uc774 \ub420\uc9c0\ub77c", "url": "https://www.wordproject.org/bibles/kr/36/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc2a4\ubc14\ub0d0", "chapter": "1", "line": 18, "content": "\uadf8\ub4e4\uc758 \uc740\uacfc \uae08\uc774 \uc5ec\ud638\uc640\uc758 \ubd84\ub178\uc758 \ub0a0\uc5d0 \ub2a5\ud788 \uadf8\ub4e4\uc744 \uac74\uc9c0\uc9c0 \ubabb \ud560 \uac83\uc774\uba70 \uc774 \uc628 \ub545\uc774 \uc5ec\ud638\uc640\uc758 \uc9c8\ud22c\uc758 \ubd88\uc5d0 \uc0bc\ud0a4\uc6b0\ub9ac\ub2c8 \uc774\ub294 \uc5