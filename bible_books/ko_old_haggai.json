[{"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 1, "content": "\ub2e4\ub9ac\uc624\uc655 \uc774\ub144 \uc720\uc6d4 \uace7 \uadf8 \ub2ec \ucd08\ud558\ub8e8\uc5d0 \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774 \uc120\uc9c0\uc790 \ud559\uac1c\ub85c \ub9d0\ubbf8\uc554\uc544 \uc2a4\uc54c\ub514\uc5d8\uc758 \uc544\ub4e4 \uc720\ub2e4 \ucd1d\ub3c5 \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc0ac\ub2e5\uc758 \uc544\ub4e4 \ub300\uc81c\uc0ac\uc7a5 \uc5ec\ud638\uc218\uc544\uc5d0\uac8c \uc784\ud558\ub2c8\ub77c \uac00\ub77c\uc0ac\ub300", "content_old": "1 \ub2e4\ub9ac\uc624\uc655 \uc774\ub144 \uc720\uc6d4 \uace7 \uadf8 \ub2ec \ucd08\ud558\ub8e8\uc5d0 \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774 \uc120\uc9c0\uc790 \ud559\uac1c\ub85c \ub9d0\ubbf8\uc554\uc544 \uc2a4\uc54c\ub514\uc5d8\uc758 \uc544\ub4e4 \uc720\ub2e4 \ucd1d\ub3c5 \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc0ac\ub2e5\uc758 \uc544\ub4e4 \ub300\uc81c\uc0ac\uc7a5 \uc5ec\ud638\uc218\uc544\uc5d0\uac8c \uc784\ud558\ub2c8\ub77c \uac00\ub77c\uc0ac\ub300", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 2, "content": "\ub9cc\uad70\uc758 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\uc5ec \uc774\ub974\ub178\ub77c \uc774 \ubc31\uc131\uc774 \ub9d0\ud558\uae30\ub97c \uc5ec\ud638\uc640\uc758 \uc804\uc744 \uac74\ucd95\ud560 \uc2dc\uae30\uac00 \uc774\ub974\uc9c0 \uc544\ub2c8\ud558\uc600\ub2e4 \ud558\ub290\ub2c8\ub77c", "content_old": "2 \ub9cc\uad70\uc758 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\uc5ec \uc774\ub974\ub178\ub77c \uc774 \ubc31\uc131\uc774 \ub9d0\ud558\uae30\ub97c \uc5ec\ud638\uc640\uc758 \uc804\uc744 \uac74\ucd95\ud560 \uc2dc\uae30\uac00 \uc774\ub974\uc9c0 \uc544\ub2c8\ud558\uc600\ub2e4 \ud558\ub290\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 3, "content": "\uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774 \uc120\uc9c0\uc790 \ud559\uac1c\uc5d0\uac8c \uc784\ud558\uc5ec \uac00\ub77c\uc0ac\ub300", "content_old": "3 \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774 \uc120\uc9c0\uc790 \ud559\uac1c\uc5d0\uac8c \uc784\ud558\uc5ec \uac00\ub77c\uc0ac\ub300", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 4, "content": "\uc774 \uc804\uc774 \ud669\ubb34\ud558\uc600\uac70\ub298 \ub108\ud76c\uac00 \uc774 \ub54c\uc5d0 \ud310\ubcbd\ud55c \uc9d1\uc5d0 \uac70\ud558\ub294 \uac83\uc774 \uac00\ud558\ub0d0", "content_old": "4 \uc774 \uc804\uc774 \ud669\ubb34\ud558\uc600\uac70\ub298 \ub108\ud76c\uac00 \uc774 \ub54c\uc5d0 \ud310\ubcbd\ud55c \uc9d1\uc5d0 \uac70\ud558\ub294 \uac83\uc774 \uac00\ud558\ub0d0", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 5, "content": "\uadf8\ub7ec\ubbc0\ub85c \uc774\uc81c \ub098 \ub9cc\uad70\uc758 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\ub178\ub2c8 \ub108\ud76c\ub294 \uc790\uae30\uc758 \uc18c\uc704\ub97c \uc0b4\ud3b4 \ubcfc\uc9c0\ub2c8\ub77c", "content_old": "5 \uadf8\ub7ec\ubbc0\ub85c \uc774\uc81c \ub098 \ub9cc\uad70\uc758 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\ub178\ub2c8 \ub108\ud76c\ub294 \uc790\uae30\uc758 \uc18c\uc704\ub97c \uc0b4\ud3b4 \ubcfc\uc9c0\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 6, "content": "\ub108\ud76c\uac00 \ub9ce\uc774 \ubfcc\ub9b4\uc9c0\ub77c\ub3c4 \uc218\uc785\uc774 \uc801\uc73c\uba70 \uba39\uc744\uc9c0\ub77c\ub3c4 \ubc30\ubd80\ub974\uc9c0 \ubabb\ud558\uba70 \ub9c8\uc2e4\uc9c0\ub77c\ub3c4 \ud761\uc871\ud558\uc9c0 \ubabb\ud558\uba70 \uc785\uc5b4\ub3c4 \ub530\ub73b\ud558\uc9c0 \ubabb\ud558\uba70 \uc77c\uad70\uc774 \uc0af\uc744 \ubc1b\uc544\ub3c4 \uadf8\uac83\uc744 \uad6c\uba4d \ub6ab\uc5b4\uc9c4 \uc804\ub300\uc5d0 \ub123\uc74c\uc774 \ub418\ub290\ub2c8\ub77c", "content_old": "6 \ub108\ud76c\uac00 \ub9ce\uc774 \ubfcc\ub9b4\uc9c0\ub77c\ub3c4 \uc218\uc785\uc774 \uc801\uc73c\uba70 \uba39\uc744\uc9c0\ub77c\ub3c4 \ubc30\ubd80\ub974\uc9c0 \ubabb\ud558\uba70 \ub9c8\uc2e4\uc9c0\ub77c\ub3c4 \ud761\uc871\ud558\uc9c0 \ubabb\ud558\uba70 \uc785\uc5b4\ub3c4 \ub530\ub73b\ud558\uc9c0 \ubabb\ud558\uba70 \uc77c\uad70\uc774 \uc0af\uc744 \ubc1b\uc544\ub3c4 \uadf8\uac83\uc744 \uad6c\uba4d \ub6ab\uc5b4\uc9c4 \uc804\ub300\uc5d0 \ub123\uc74c\uc774 \ub418\ub290\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 7, "content": "\ub098 \ub9cc\uad70\uc758 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\ub178\ub2c8 \ub108\ud76c\ub294 \uc790\uae30\uc758 \uc18c\uc704\ub97c \uc0b4\ud3b4 \ubcfc\uc9c0\ub2c8\ub77c", "content_old": "7 \ub098 \ub9cc\uad70\uc758 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\ub178\ub2c8 \ub108\ud76c\ub294 \uc790\uae30\uc758 \uc18c\uc704\ub97c \uc0b4\ud3b4 \ubcfc\uc9c0\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 8, "content": "\ub108\ud76c\ub294 \uc0b0\uc5d0 \uc62c\ub77c\uac00\uc11c \ub098\ubb34\ub97c \uac00\uc838\ub2e4\uac00 \uc804\uc744 \uac74\ucd95\ud558\ub77c \uadf8\ub9ac\ud558\uba74 \ub0b4\uac00 \uadf8\ub85c \uc778\ud558\uc5ec \uae30\ubed0\ud558\uace0 \ub610 \uc601\uad11\uc744 \uc5bb\uc73c\ub9ac\ub77c \ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\uc600\ub290\ub2c8\ub77c", "content_old": "8 \ub108\ud76c\ub294 \uc0b0\uc5d0 \uc62c\ub77c\uac00\uc11c \ub098\ubb34\ub97c \uac00\uc838\ub2e4\uac00 \uc804\uc744 \uac74\ucd95\ud558\ub77c \uadf8\ub9ac\ud558\uba74 \ub0b4\uac00 \uadf8\ub85c \uc778\ud558\uc5ec \uae30\ubed0\ud558\uace0 \ub610 \uc601\uad11\uc744 \uc5bb\uc73c\ub9ac\ub77c \ub098 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\uc600\ub290\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 9, "content": "\ub108\ud76c\uac00 \ub9ce\uc740 \uac83\uc744 \ubc14\ub790\uc73c\ub098 \ub3c4\ub9ac\uc5b4 \uc801\uc5c8\uace0 \ub108\ud76c\uac00 \uadf8\uac83\uc744 \uc9d1\uc73c\ub85c \uac00\uc838\uac14\uc73c\ub098 \ub0b4\uac00 \ubd88\uc5b4 \ubc84\ub838\ub290\ub2c8\ub77c \ub098 \ub9cc\uad70\uc758 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\ub178\ub77c \uc774\uac83\uc774 \ubb34\uc2a8 \uc5f0\uace0\ub1e8 \ub0b4 \uc9d1\uc740 \ud669\ubb34\ud558\uc600\uc73c\ub418 \ub108\ud76c\ub294 \uac01\uac01 \uc790\uae30\uc758 \uc9d1\uc5d0 \ube68\ub790\uc74c\uc774\ub2c8\ub77c", "content_old": "9 \ub108\ud76c\uac00 \ub9ce\uc740 \uac83\uc744 \ubc14\ub790\uc73c\ub098 \ub3c4\ub9ac\uc5b4 \uc801\uc5c8\uace0 \ub108\ud76c\uac00 \uadf8\uac83\uc744 \uc9d1\uc73c\ub85c \uac00\uc838\uac14\uc73c\ub098 \ub0b4\uac00 \ubd88\uc5b4 \ubc84\ub838\ub290\ub2c8\ub77c \ub098 \ub9cc\uad70\uc758 \uc5ec\ud638\uc640\uac00 \ub9d0\ud558\ub178\ub77c \uc774\uac83\uc774 \ubb34\uc2a8 \uc5f0\uace0\ub1e8 \ub0b4 \uc9d1\uc740 \ud669\ubb34\ud558\uc600\uc73c\ub418 \ub108\ud76c\ub294 \uac01\uac01 \uc790\uae30\uc758 \uc9d1\uc5d0 \ube68\ub790\uc74c\uc774\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 10, "content": "\uadf8\ub7ec\ubbc0\ub85c \ub108\ud76c\ub85c \uc778\ud558\uc5ec \ud558\ub298\uc740 \uc774\uc2ac\uc744 \uadf8\ucce4\uace0 \ub545\uc740 \uc0b0\ubb3c\uc744 \uadf8\ucce4\uc73c\uba70", "content_old": "10 \uadf8\ub7ec\ubbc0\ub85c \ub108\ud76c\ub85c \uc778\ud558\uc5ec \ud558\ub298\uc740 \uc774\uc2ac\uc744 \uadf8\ucce4\uace0 \ub545\uc740 \uc0b0\ubb3c\uc744 \uadf8\ucce4\uc73c\uba70", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 11, "content": "\"\ub0b4\uac00 \ud55c\uc7ac\ub97c \ubd88\ub7ec \uc774 \ub545\uc5d0, \uc0b0\uc5d0, \uace1\ubb3c\uc5d0, \uc0c8 \ud3ec\ub3c4\uc8fc\uc5d0, \uae30\ub984\uc5d0, \ub545\uc758 \ubaa8\ub4e0 \uc18c\uc0b0\uc5d0, \uc0ac\ub78c\uc5d0\uac8c, \uc721\ucd95\uc5d0\uac8c, \uc190\uc73c\ub85c \uc218\uace0\ud558\ub294 \ubaa8\ub4e0 \uc77c\uc5d0 \uc784\ud558\uac8c \ud558\uc600\ub290\ub2c8\ub77c\"", "content_old": "11 \"\ub0b4\uac00 \ud55c\uc7ac\ub97c \ubd88\ub7ec \uc774 \ub545\uc5d0, \uc0b0\uc5d0, \uace1\ubb3c\uc5d0, \uc0c8 \ud3ec\ub3c4\uc8fc\uc5d0, \uae30\ub984\uc5d0, \ub545\uc758 \ubaa8\ub4e0 \uc18c\uc0b0\uc5d0, \uc0ac\ub78c\uc5d0\uac8c, \uc721\ucd95\uc5d0\uac8c, \uc190\uc73c\ub85c \uc218\uace0\ud558\ub294 \ubaa8\ub4e0 \uc77c\uc5d0 \uc784\ud558\uac8c \ud558\uc600\ub290\ub2c8\ub77c\"", "url": "https://www.wordproject.org/bibles/kr/37/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\ud559\uac1c", "chapter": "1", "line": 12, "content": "\uc2a4\uc54c\ub514\uc5d8\uc758 \uc544\ub4e4 \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc0ac\ub2e5\uc758 \uc544\ub4e4 \ub300\uc81c\uc0ac\uc7a5 \uc5ec\ud638\uc218\uc544\uc640 \ub0a8\uc740\ubc14 \ubaa8\ub4e0 \ubc31\uc131\uc774 \uadf8 \ud558\ub098\ub2d8 \uc5ec\ud638\uc640\uc758 \ubaa9\uc18c\ub9ac\uc640 \uc120\uc9c0\uc790 \ud559\uac1c\uc758 \ub9d0\uc744 \uccad\uc885\ud558\uc600\uc73c\ub2c8 \uc774\ub294 \uadf8\ub4e4\uc758 \ud558\ub098\ub2d8 \uc5ec\ud638\uc640\uaed8\uc11c \uadf8\ub97c \ubcf4\ub0b4\uc168\uc74c\uc744 \uc778\ud568\uc774\ub77c \ubc31\uc131\uc774 \ub2e4 \uc5ec\ud638\uc640\ub97c \uacbd\uc678\ud558\ub9e4", "content_old": "12 \uc2a4\uc54c\ub514\uc5d8\uc758 \uc544\ub4e4 \uc2a4\ub8f9\ubc14\ubca8\uacfc \uc5ec\ud638\uc0ac\ub2e5\uc758 \uc544\ub4e4 \ub300\uc81c\uc0ac\uc7a5 \uc5ec\ud638\uc218\uc544\uc640 \ub0a8\uc740\ubc14 \ubaa8\ub4e0 \ubc31\uc131\uc774 \uadf8 \ud558\ub098\ub2d8 \uc5ec\ud638\uc640\uc758 \ubaa9\uc18c\ub9ac\uc640 \uc120\uc9c0\uc790 \ud559\uac1c\uc758 \ub9d0\uc744 \uccad\uc885\ud558\uc600\uc73c\ub2c8 \uc774\ub294 \uadf8\ub4e4\uc758 \ud558\ub098\ub2d8 \uc5ec\ud638\uc640\uaed8\