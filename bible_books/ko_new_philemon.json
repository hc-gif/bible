[{"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 1, "content": "\uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub97c \uc704\ud558\uc5ec \uac07\ud78c \uc790 \ub41c \ubc14\uc6b8\uacfc \ubc0f \ud615\uc81c \ub514\ubaa8\ub370\ub294 \uc6b0\ub9ac\uc758 \uc0ac\ub791\uc744 \ubc1b\ub294 \uc790\uc694 \ub3d9\uc5ed\uc790\uc778 \ube4c\ub808\ubaac\uacfc", "content_old": "1 \uadf8\ub9ac\uc2a4\ub3c4 \uc608\uc218\ub97c \uc704\ud558\uc5ec \uac07\ud78c \uc790 \ub41c \ubc14\uc6b8\uacfc \ubc0f \ud615\uc81c \ub514\ubaa8\ub370\ub294 \uc6b0\ub9ac\uc758 \uc0ac\ub791\uc744 \ubc1b\ub294 \uc790\uc694 \ub3d9\uc5ed\uc790\uc778 \ube4c\ub808\ubaac\uacfc", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 2, "content": "\ubc0f \uc790\ub9e4 \uc555\ube44\uc544\uc640 \ubc0f \uc6b0\ub9ac\uc640 \ud568\uaed8 \uad70\uc0ac \ub41c \uc544\ud0b5\ubcf4\uc640 \ub124 \uc9d1\uc5d0 \uc788\ub294 \uad50\ud68c\uc5d0\uac8c \ud3b8\uc9c0\ud558\ub178\ub2c8", "content_old": "2 \ubc0f \uc790\ub9e4 \uc555\ube44\uc544\uc640 \ubc0f \uc6b0\ub9ac\uc640 \ud568\uaed8 \uad70\uc0ac \ub41c \uc544\ud0b5\ubcf4\uc640 \ub124 \uc9d1\uc5d0 \uc788\ub294 \uad50\ud68c\uc5d0\uac8c \ud3b8\uc9c0\ud558\ub178\ub2c8", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 3, "content": "\ud558\ub098\ub2d8 \uc6b0\ub9ac \uc544\ubc84\uc9c0\uc640 \uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\ub85c \uc887\uc544 \uc740\ud61c\uc640 \ud3c9\uac15\uc774 \ub108\ud76c\uc5d0\uac8c \uc788\uc744\uc9c0\uc5b4\ub2e4", "content_old": "3 \ud558\ub098\ub2d8 \uc6b0\ub9ac \uc544\ubc84\uc9c0\uc640 \uc8fc \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\ub85c \uc887\uc544 \uc740\ud61c\uc640 \ud3c9\uac15\uc774 \ub108\ud76c\uc5d0\uac8c \uc788\uc744\uc9c0\uc5b4\ub2e4", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 4, "content": "\ub0b4\uac00 \ud56d\uc0c1 \ub0b4 \ud558\ub098\ub2d8\uaed8 \uac10\uc0ac\ud558\uace0 \uae30\ub3c4\ud560 \ub54c\uc5d0 \ub108\ub97c \ub9d0\ud568\uc740", "content_old": "4 \ub0b4\uac00 \ud56d\uc0c1 \ub0b4 \ud558\ub098\ub2d8\uaed8 \uac10\uc0ac\ud558\uace0 \uae30\ub3c4\ud560 \ub54c\uc5d0 \ub108\ub97c \ub9d0\ud568\uc740", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 5, "content": "\uc8fc \uc608\uc218\uc640 \ubc0f \ubaa8\ub4e0 \uc131\ub3c4\uc5d0 \ub300\ud55c \ub124 \uc0ac\ub791\uacfc \ubbff\uc74c\uc774 \uc788\uc74c\uc744 \ub4e4\uc74c\uc774\ub2c8", "content_old": "5 \uc8fc \uc608\uc218\uc640 \ubc0f \ubaa8\ub4e0 \uc131\ub3c4\uc5d0 \ub300\ud55c \ub124 \uc0ac\ub791\uacfc \ubbff\uc74c\uc774 \uc788\uc74c\uc744 \ub4e4\uc74c\uc774\ub2c8", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 6, "content": "\uc774\ub85c\uc368 \ub124 \ubbff\uc74c\uc758 \uad50\uc81c\uac00 \uc6b0\ub9ac \uac00\uc6b4\ub370 \uc788\ub294 \uc120\uc744 \uc54c\uac8c \ud558\uace0 \uadf8\ub9ac\uc2a4\ub3c4\uaed8 \ubbf8\uce58\ub3c4\ub85d \uc5ed\uc0ac\ud558\ub290\ub2c8\ub77c", "content_old": "6 \uc774\ub85c\uc368 \ub124 \ubbff\uc74c\uc758 \uad50\uc81c\uac00 \uc6b0\ub9ac \uac00\uc6b4\ub370 \uc788\ub294 \uc120\uc744 \uc54c\uac8c \ud558\uace0 \uadf8\ub9ac\uc2a4\ub3c4\uaed8 \ubbf8\uce58\ub3c4\ub85d \uc5ed\uc0ac\ud558\ub290\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 7, "content": "\ud615\uc81c\uc5ec \uc131\ub3c4\ub4e4\uc758 \ub9c8\uc74c\uc774 \ub108\ub85c \ub9d0\ubbf8\uc554\uc544 \ud3c9\uc548\ud568\uc744 \uc5bb\uc5c8\uc73c\ub2c8 \ub0b4\uac00 \ub108\uc758 \uc0ac\ub791\uc73c\ub85c \ub9ce\uc740 \uae30\uc068\uacfc \uc704\ub85c\ub97c \uc5bb\uc5c8\ub178\ub77c", "content_old": "7 \ud615\uc81c\uc5ec \uc131\ub3c4\ub4e4\uc758 \ub9c8\uc74c\uc774 \ub108\ub85c \ub9d0\ubbf8\uc554\uc544 \ud3c9\uc548\ud568\uc744 \uc5bb\uc5c8\uc73c\ub2c8 \ub0b4\uac00 \ub108\uc758 \uc0ac\ub791\uc73c\ub85c \ub9ce\uc740 \uae30\uc068\uacfc \uc704\ub85c\ub97c \uc5bb\uc5c8\ub178\ub77c", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 8, "content": "\uc774\ub7ec\ubbc0\ub85c \ub0b4\uac00 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \ub9ce\uc740 \ub2f4\ub825\uc744 \uac00\uc9c0\uace0 \ub124\uac8c \ub9c8\ub545\ud55c \uc77c\ub85c \uba85\ud560 \uc218 \uc788\uc73c\ub098", "content_old": "8 \uc774\ub7ec\ubbc0\ub85c \ub0b4\uac00 \uadf8\ub9ac\uc2a4\ub3c4 \uc548\uc5d0\uc11c \ub9ce\uc740 \ub2f4\ub825\uc744 \uac00\uc9c0\uace0 \ub124\uac8c \ub9c8\ub545\ud55c \uc77c\ub85c \uba85\ud560 \uc218 \uc788\uc73c\ub098", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 9, "content": "\uc0ac\ub791\uc744 \uc778\ud558\uc5ec \ub3c4\ub9ac\uc5b4 \uac04\uad6c\ud558\ub178\ub2c8 \ub098\uc774 \ub9ce\uc740 \ub098 \ubc14\uc6b8\uc740 \uc9c0\uae08 \ub610 \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc704\ud558\uc5ec \uac07\ud78c \uc790 \ub418\uc5b4", "content_old": "9 \uc0ac\ub791\uc744 \uc778\ud558\uc5ec \ub3c4\ub9ac\uc5b4 \uac04\uad6c\ud558\ub178\ub2c8 \ub098\uc774 \ub9ce\uc740 \ub098 \ubc14\uc6b8\uc740 \uc9c0\uae08 \ub610 \uc608\uc218 \uadf8\ub9ac\uc2a4\ub3c4\ub97c \uc704\ud558\uc5ec \uac07\ud78c \uc790 \ub418\uc5b4", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 10, "content": "\uac07\ud78c \uc911\uc5d0\uc11c \ub0b3\uc740 \uc544\ub4e4 \uc624\ub124\uc2dc\ubaa8\ub97c \uc704\ud558\uc5ec \ub124\uac8c \uac04\uad6c\ud558\ub178\ub77c", "content_old": "10 \uac07\ud78c \uc911\uc5d0\uc11c \ub0b3\uc740 \uc544\ub4e4 \uc624\ub124\uc2dc\ubaa8\ub97c \uc704\ud558\uc5ec \ub124\uac8c \uac04\uad6c\ud558\ub178\ub77c", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 11, "content": "\uc800\uac00 \uc804\uc5d0\ub294 \ub124\uac8c \ubb34\uc775\ud558\uc600\uc73c\ub098 \uc774\uc81c\ub294 \ub098\uc640 \ub124\uac8c \uc720\uc775\ud558\ubbc0\ub85c", "content_old": "11 \uc800\uac00 \uc804\uc5d0\ub294 \ub124\uac8c \ubb34\uc775\ud558\uc600\uc73c\ub098 \uc774\uc81c\ub294 \ub098\uc640 \ub124\uac8c \uc720\uc775\ud558\ubbc0\ub85c", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 12, "content": "\ub124\uac8c \uc800\ub97c \ub3cc\ub824 \ubcf4\ub0b4\ub178\ub2c8 \uc800\ub294 \ub0b4 \uc2ec\ubcf5\uc774\ub77c", "content_old": "12 \ub124\uac8c \uc800\ub97c \ub3cc\ub824 \ubcf4\ub0b4\ub178\ub2c8 \uc800\ub294 \ub0b4 \uc2ec\ubcf5\uc774\ub77c", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 13, "content": "\uc800\ub97c \ub0b4\uac8c \uba38\ubb3c\ub7ec \ub450\uc5b4 \ub0b4 \ubcf5\uc74c\uc744 \uc704\ud558\uc5ec \uac07\ud78c \uc911\uc5d0\uc11c \ub124 \ub300\uc2e0 \ub098\ub97c \uc12c\uae30\uac8c \ud558\uace0\uc790 \ud558\ub098", "content_old": "13 \uc800\ub97c \ub0b4\uac8c \uba38\ubb3c\ub7ec \ub450\uc5b4 \ub0b4 \ubcf5\uc74c\uc744 \uc704\ud558\uc5ec \uac07\ud78c \uc911\uc5d0\uc11c \ub124 \ub300\uc2e0 \ub098\ub97c \uc12c\uae30\uac8c \ud558\uace0\uc790 \ud558\ub098", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 14, "content": "\ub2e4\ub9cc \ub124 \uc2b9\ub099\uc774 \uc5c6\uc774\ub294 \ub0b4\uac00 \uc544\ubb34 \uac83\ub3c4 \ud558\uae30\ub97c \uc6d0\uce58 \uc544\ub2c8\ud558\ub178\ub2c8 \uc774\ub294 \ub108\uc758 \uc120\ud55c \uc77c\uc774 \uc5b5\uc9c0\uac19\uc774 \ub418\uc9c0 \uc544\ub2c8\ud558\uace0 \uc790\uc758\ub85c \ub418\uac8c \ud558\ub824 \ud568\uc774\ub85c\ub77c", "content_old": "14 \ub2e4\ub9cc \ub124 \uc2b9\ub099\uc774 \uc5c6\uc774\ub294 \ub0b4\uac00 \uc544\ubb34 \uac83\ub3c4 \ud558\uae30\ub97c \uc6d0\uce58 \uc544\ub2c8\ud558\ub178\ub2c8 \uc774\ub294 \ub108\uc758 \uc120\ud55c \uc77c\uc774 \uc5b5\uc9c0\uac19\uc774 \ub418\uc9c0 \uc544\ub2c8\ud558\uace0 \uc790\uc758\ub85c \ub418\uac8c \ud558\ub824 \ud568\uc774\ub85c\ub77c", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 15, "content": "\uc800\uac00 \uc7a0\uc2dc \ub5a0\ub098\uac8c \ub41c \uac83\uc740 \uc774\ub97c \uc778\ud558\uc5ec \uc800\ub97c \uc601\uc6d0\ud788 \ub450\uac8c \ud568\uc774\ub2c8", "content_old": "15 \uc800\uac00 \uc7a0\uc2dc \ub5a0\ub098\uac8c \ub41c \uac83\uc740 \uc774\ub97c \uc778\ud558\uc5ec \uc800\ub97c \uc601\uc6d0\ud788 \ub450\uac8c \ud568\uc774\ub2c8", "url": "https://www.wordproject.org/bibles/kr/57/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 1, "book": "\ube4c\ub808\ubaac\uc11c", "chapter": "1", "line": 16, "content": "\uc774 \ud6c4\ub85c\ub294 \uc885\uacfc \uac19\uc774 \uc544\ub2c8\ud558\uace0 \uc885\uc5d0\uc11c \ub6f0\uc5b4\ub098 \uace7 \uc0ac\ub791 \ubc1b\ub294 \ud615\uc81c\ub85c \ub458 \uc790\ub77c \ub0b4\uac8c \ud2b9\ubcc4\ud788 \uadf8\ub7ec\ud558\uac70\ub4e0 \ud558\ubb3c\uba70 \uc721\uc2e0\uacfc \uc8fc \uc548\uc5d0\uc11c \uc0c1\uad00\ub41c \ub124\uac8c\ub7b4", "content_old": "16 \uc774 \ud6c4\ub85c\ub294 \uc885\uacfc \uac19\uc774 \uc544\ub2c8\ud558\uace0 \uc885\uc5d0\uc11c \ub6f0\uc5b4\ub098 \uace7 \uc0ac\ub791 \ubc1b\ub294 \ud615\uc81c\ub85c \ub458 \uc790\ub77c \ub0b4\uac8c \ud