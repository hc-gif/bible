[{"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 1, "content": "\uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774 \uc544\ubc0b\ub300\uc758 \uc544\ub4e4 \uc694\ub098\uc5d0\uac8c \uc784\ud558\ub2c8\ub77c \uc774\ub974\uc2dc\ub418", "content_old": "1 \uc5ec\ud638\uc640\uc758 \ub9d0\uc500\uc774 \uc544\ubc0b\ub300\uc758 \uc544\ub4e4 \uc694\ub098\uc5d0\uac8c \uc784\ud558\ub2c8\ub77c \uc774\ub974\uc2dc\ub418", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 2, "content": "\ub108\ub294 \uc77c\uc5b4\ub098 \uc800 \ud070 \uc131\uc74d \ub2c8\ub290\uc6e8\ub85c \uac00\uc11c \uadf8\uac83\uc744 \uccd0\uc11c \uc678\uce58\ub77c \uadf8 \uc545\ub3c5\uc774 \ub0b4 \uc55e\uc5d0 \uc0c1\ub2ec\ud558\uc600\uc74c\uc774\ub2c8\ub77c \ud558\uc2dc\ub2c8\ub77c", "content_old": "2 \ub108\ub294 \uc77c\uc5b4\ub098 \uc800 \ud070 \uc131\uc74d \ub2c8\ub290\uc6e8\ub85c \uac00\uc11c \uadf8\uac83\uc744 \uccd0\uc11c \uc678\uce58\ub77c \uadf8 \uc545\ub3c5\uc774 \ub0b4 \uc55e\uc5d0 \uc0c1\ub2ec\ud558\uc600\uc74c\uc774\ub2c8\ub77c \ud558\uc2dc\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 3, "content": "\uadf8\ub7ec\ub098 \uc694\ub098\uac00 \uc5ec\ud638\uc640\uc758 \ub0af\uc744 \ud53c\ud558\ub824\uace0 \uc77c\uc5b4\ub098 \ub2e4\uc2dc\uc2a4\ub85c \ub3c4\ub9dd\ud558\ub824 \ud558\uc5ec \uc6a5\ubc14\ub85c \ub0b4\ub824\uac14\ub354\ub2c8 \ub9c8\uce68 \ub2e4\uc2dc\uc2a4\ub85c \uac00\ub294 \ubc30\ub97c \ub9cc\ub09c\uc9c0\ub77c \uc5ec\ud638\uc640\uc758 \ub0af\uc744 \ud53c\ud558\uc5ec \ud568\uaed8 \ub2e4\uc2dc\uc2a4\ub85c \uac00\ub824\uace0 \uc120\uac00\ub97c \uc8fc\uace0 \ubc30\uc5d0 \uc62c\ub790\ub354\ub77c", "content_old": "3 \uadf8\ub7ec\ub098 \uc694\ub098\uac00 \uc5ec\ud638\uc640\uc758 \ub0af\uc744 \ud53c\ud558\ub824\uace0 \uc77c\uc5b4\ub098 \ub2e4\uc2dc\uc2a4\ub85c \ub3c4\ub9dd\ud558\ub824 \ud558\uc5ec \uc6a5\ubc14\ub85c \ub0b4\ub824\uac14\ub354\ub2c8 \ub9c8\uce68 \ub2e4\uc2dc\uc2a4\ub85c \uac00\ub294 \ubc30\ub97c \ub9cc\ub09c\uc9c0\ub77c \uc5ec\ud638\uc640\uc758 \ub0af\uc744 \ud53c\ud558\uc5ec \ud568\uaed8 \ub2e4\uc2dc\uc2a4\ub85c \uac00\ub824\uace0 \uc120\uac00\ub97c \uc8fc\uace0 \ubc30\uc5d0 \uc62c\ub790\ub354\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 4, "content": "\uc5ec\ud638\uc640\uaed8\uc11c \ub300\ud48d\uc744 \ubc14\ub2e4\uc704\uc5d0 \ub0b4\ub9ac\uc2dc\ub9e4 \ubc14\ub2e4 \uac00\uc6b4\ub370 \ud3ed\ud48d\uc774 \ub300\uc791\ud558\uc5ec \ubc30\uac00 \uac70\uc758 \uae68\uc5b4\uc9c0\uac8c \ub41c\uc9c0\ub77c", "content_old": "4 \uc5ec\ud638\uc640\uaed8\uc11c \ub300\ud48d\uc744 \ubc14\ub2e4\uc704\uc5d0 \ub0b4\ub9ac\uc2dc\ub9e4 \ubc14\ub2e4 \uac00\uc6b4\ub370 \ud3ed\ud48d\uc774 \ub300\uc791\ud558\uc5ec \ubc30\uac00 \uac70\uc758 \uae68\uc5b4\uc9c0\uac8c \ub41c\uc9c0\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 5, "content": "\uc0ac\uacf5\uc774 \ub450\ub824\uc6cc \ud558\uc5ec \uac01\uac01 \uc790\uae30\uc758 \uc2e0\uc744 \ubd80\ub974\uace0 \ub610 \ubc30\ub97c \uac00\ubccd\uac8c \ud558\ub824\uace0 \uadf8 \uac00\uc6b4\ub370 \ubb3c\uac74\uc744 \ubc14\ub2e4\uc5d0 \ub358\uc9c0\ub2c8\ub77c \uadf8\ub7ec\ub098 \uc694\ub098\ub294 \ubc30 \ubc11\uce35\uc5d0 \ub0b4\ub824\uac00\uc11c \ub204\uc6cc \uae4a\uc774 \uc7a0\uc774 \ub4e0\uc9c0\ub77c", "content_old": "5 \uc0ac\uacf5\uc774 \ub450\ub824\uc6cc \ud558\uc5ec \uac01\uac01 \uc790\uae30\uc758 \uc2e0\uc744 \ubd80\ub974\uace0 \ub610 \ubc30\ub97c \uac00\ubccd\uac8c \ud558\ub824\uace0 \uadf8 \uac00\uc6b4\ub370 \ubb3c\uac74\uc744 \ubc14\ub2e4\uc5d0 \ub358\uc9c0\ub2c8\ub77c \uadf8\ub7ec\ub098 \uc694\ub098\ub294 \ubc30 \ubc11\uce35\uc5d0 \ub0b4\ub824\uac00\uc11c \ub204\uc6cc \uae4a\uc774 \uc7a0\uc774 \ub4e0\uc9c0\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 6, "content": "\uc120\uc7a5\uc774 \ub098\uc544\uac00\uc11c \uadf8\uc5d0\uac8c \uc774\ub974\ub418 \uc790\ub294 \uc790\uc5ec \uc5b4\ucc1c\uc774\ub1e8 \uc77c\uc5b4\ub098\uc11c \ub124 \ud558\ub098\ub2d8\uaed8 \uad6c\ud558\ub77c \ud639\uc2dc \ud558\ub098\ub2d8\uc774 \uc6b0\ub9ac\ub97c \uc0dd\uac01\ud558\uc0ac \ub9dd\ud558\uc9c0 \uc54a\uac8c \ud558\uc2dc\ub9ac\ub77c \ud558\ub2c8\ub77c", "content_old": "6 \uc120\uc7a5\uc774 \ub098\uc544\uac00\uc11c \uadf8\uc5d0\uac8c \uc774\ub974\ub418 \uc790\ub294 \uc790\uc5ec \uc5b4\ucc1c\uc774\ub1e8 \uc77c\uc5b4\ub098\uc11c \ub124 \ud558\ub098\ub2d8\uaed8 \uad6c\ud558\ub77c \ud639\uc2dc \ud558\ub098\ub2d8\uc774 \uc6b0\ub9ac\ub97c \uc0dd\uac01\ud558\uc0ac \ub9dd\ud558\uc9c0 \uc54a\uac8c \ud558\uc2dc\ub9ac\ub77c \ud558\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 7, "content": "\uadf8\ub4e4\uc774 \uc11c\ub85c \uc774\ub974\ub418 \uc790 \uc6b0\ub9ac\uac00 \uc81c\ube44\ub97c \ubf51\uc544 \uc774 \uc7ac\uc559\uc774 \ub204\uad6c\ub85c \uc778\ud558\uc5ec \uc6b0\ub9ac\uc5d0\uac8c \uc784\ud558\uc600\ub098 \uc54c\uc790 \ud558\uace0 \uace7 \uc81c\ube44\ub97c \ubf51\uc73c\ub2c8 \uc81c\ube44\uac00 \uc694\ub098\uc5d0\uac8c \ub2f9\ud55c\uc9c0\ub77c", "content_old": "7 \uadf8\ub4e4\uc774 \uc11c\ub85c \uc774\ub974\ub418 \uc790 \uc6b0\ub9ac\uac00 \uc81c\ube44\ub97c \ubf51\uc544 \uc774 \uc7ac\uc559\uc774 \ub204\uad6c\ub85c \uc778\ud558\uc5ec \uc6b0\ub9ac\uc5d0\uac8c \uc784\ud558\uc600\ub098 \uc54c\uc790 \ud558\uace0 \uace7 \uc81c\ube44\ub97c \ubf51\uc73c\ub2c8 \uc81c\ube44\uac00 \uc694\ub098\uc5d0\uac8c \ub2f9\ud55c\uc9c0\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 8, "content": "\ubb34\ub9ac\uac00 \uadf8\uc5d0\uac8c \uc774\ub974\ub418 \uccad\ucee8\ub300 \uc774 \uc7ac\uc559\uc774 \ubb34\uc2a8 \uc5f0\uace0\ub85c \uc6b0\ub9ac\uc5d0\uac8c \uc784\ud558\uc600\ub294\uac00 \uace0\ud558\ub77c \ub124 \uc0dd\uc5c5\uc774 \ubb34\uc5c7\uc774\uba70 \uc5b4\ub514\uc11c \uc654\uc73c\uba70 \uace0\uad6d\uc774 \uc5b4\ub514\uba70 \uc5b4\ub290 \ubbfc\uc871\uc5d0 \uc18d\ud558\uc600\ub290\ub0d0", "content_old": "8 \ubb34\ub9ac\uac00 \uadf8\uc5d0\uac8c \uc774\ub974\ub418 \uccad\ucee8\ub300 \uc774 \uc7ac\uc559\uc774 \ubb34\uc2a8 \uc5f0\uace0\ub85c \uc6b0\ub9ac\uc5d0\uac8c \uc784\ud558\uc600\ub294\uac00 \uace0\ud558\ub77c \ub124 \uc0dd\uc5c5\uc774 \ubb34\uc5c7\uc774\uba70 \uc5b4\ub514\uc11c \uc654\uc73c\uba70 \uace0\uad6d\uc774 \uc5b4\ub514\uba70 \uc5b4\ub290 \ubbfc\uc871\uc5d0 \uc18d\ud558\uc600\ub290\ub0d0", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 9, "content": "\uadf8\uac00 \ub300\ub2f5\ud558\ub418 \ub098\ub294 \ud788\ube0c\ub9ac \uc0ac\ub78c\uc774\uc694 \ubc14\ub2e4\uc640 \uc721\uc9c0\ub97c \uc9c0\uc73c\uc2e0 \ud558\ub298\uc758 \ud558\ub098\ub2d8 \uc5ec\ud638\uc640\ub97c \uacbd\uc678\ud558\ub294 \uc790\ub85c\ub77c \ud558\uace0", "content_old": "9 \uadf8\uac00 \ub300\ub2f5\ud558\ub418 \ub098\ub294 \ud788\ube0c\ub9ac \uc0ac\ub78c\uc774\uc694 \ubc14\ub2e4\uc640 \uc721\uc9c0\ub97c \uc9c0\uc73c\uc2e0 \ud558\ub298\uc758 \ud558\ub098\ub2d8 \uc5ec\ud638\uc640\ub97c \uacbd\uc678\ud558\ub294 \uc790\ub85c\ub77c \ud558\uace0", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 10, "content": "\uc790\uae30\uac00 \uc5ec\ud638\uc640\uc758 \ub0af\uc744 \ud53c\ud568\uc778\uc904\uc744 \uadf8\ub4e4\uc5d0\uac8c \uace0\ud558\uc600\uc73c\ubbc0\ub85c \ubb34\ub9ac\uac00 \uc54c\uace0 \uc2ec\ud788 \ub450\ub824\uc6cc\ud558\uc5ec \uc774\ub974\ub418 \ub124\uac00 \uc5b4\ucc0c\ud558\uc5ec \uc774\ub807\uac8c \ud589\ud558\uc600\ub290\ub0d0 \ud558\ub2c8\ub77c", "content_old": "10 \uc790\uae30\uac00 \uc5ec\ud638\uc640\uc758 \ub0af\uc744 \ud53c\ud568\uc778\uc904\uc744 \uadf8\ub4e4\uc5d0\uac8c \uace0\ud558\uc600\uc73c\ubbc0\ub85c \ubb34\ub9ac\uac00 \uc54c\uace0 \uc2ec\ud788 \ub450\ub824\uc6cc\ud558\uc5ec \uc774\ub974\ub418 \ub124\uac00 \uc5b4\ucc0c\ud558\uc5ec \uc774\ub807\uac8c \ud589\ud558\uc600\ub290\ub0d0 \ud558\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 11, "content": "\ubc14\ub2e4\uac00 \uc810\uc810 \ud749\uc6a9\ud55c\uc9c0\ub77c \ubb34\ub9ac\uac00 \uadf8\uc5d0\uac8c \uc774\ub974\ub418 \uc6b0\ub9ac\uac00 \ub108\ub97c \uc5b4\ub5bb\uac8c \ud558\uc5ec\uc57c \ubc14\ub2e4\uac00 \uc6b0\ub9ac\ub97c \uc704\ud558\uc5ec \uc794\uc794\ud558\uaca0\ub290\ub0d0", "content_old": "11 \ubc14\ub2e4\uac00 \uc810\uc810 \ud749\uc6a9\ud55c\uc9c0\ub77c \ubb34\ub9ac\uac00 \uadf8\uc5d0\uac8c \uc774\ub974\ub418 \uc6b0\ub9ac\uac00 \ub108\ub97c \uc5b4\ub5bb\uac8c \ud558\uc5ec\uc57c \ubc14\ub2e4\uac00 \uc6b0\ub9ac\ub97c \uc704\ud558\uc5ec \uc794\uc794\ud558\uaca0\ub290\ub0d0", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 12, "content": "\uadf8\uac00 \ub300\ub2f5\ud558\ub418 \ub098\ub97c \ub4e4\uc5b4 \ubc14\ub2e4\uc5d0 \ub358\uc9c0\ub77c \uadf8\ub9ac\ud558\uba74 \ubc14\ub2e4\uac00 \ub108\ud76c\ub97c \uc704\ud558\uc5ec \uc794\uc794\ud558\ub9ac\ub77c \ub108\ud76c\uac00 \uc774 \ud070 \ud3ed\ud48d\uc744 \ub9cc\ub09c \uac83\uc774 \ub098\uc758 \uc5f0\uace0\uc778 \uc904\uc744 \ub0b4\uac00 \uc544\ub178\ub77c \ud558\ub2c8\ub77c", "content_old": "12 \uadf8\uac00 \ub300\ub2f5\ud558\ub418 \ub098\ub97c \ub4e4\uc5b4 \ubc14\ub2e4\uc5d0 \ub358\uc9c0\ub77c \uadf8\ub9ac\ud558\uba74 \ubc14\ub2e4\uac00 \ub108\ud76c\ub97c \uc704\ud558\uc5ec \uc794\uc794\ud558\ub9ac\ub77c \ub108\ud76c\uac00 \uc774 \ud070 \ud3ed\ud48d\uc744 \ub9cc\ub09c \uac83\uc774 \ub098\uc758 \uc5f0\uace0\uc778 \uc904\uc744 \ub0b4\uac00 \uc544\ub178\ub77c \ud558\ub2c8\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 13, "content": "\uadf8\ub7ec\ub098 \uadf8 \uc0ac\ub78c\ub4e4\uc774 \ud798\uc368 \ub178\ub97c \uc800\uc5b4 \ubc30\ub97c \uc721\uc9c0\uc5d0 \ub3cc\ub9ac\uace0\uc790 \ud558\ub2e4\uac00 \ubc14\ub2e4\uac00 \uadf8\ub4e4\uc744 \ud5a5\ud558\uc5ec \uc810\uc810 \ub354 \ud749\uc6a9\ud558\ubbc0\ub85c \ub2a5\ud788 \ubabb\ud55c\uc9c0\ub77c", "content_old": "13 \uadf8\ub7ec\ub098 \uadf8 \uc0ac\ub78c\ub4e4\uc774 \ud798\uc368 \ub178\ub97c \uc800\uc5b4 \ubc30\ub97c \uc721\uc9c0\uc5d0 \ub3cc\ub9ac\uace0\uc790 \ud558\ub2e4\uac00 \ubc14\ub2e4\uac00 \uadf8\ub4e4\uc744 \ud5a5\ud558\uc5ec \uc810\uc810 \ub354 \ud749\uc6a9\ud558\ubbc0\ub85c \ub2a5\ud788 \ubabb\ud55c\uc9c0\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 14, "content": "\ubb34\ub9ac\uac00 \uc5ec\ud638\uc640\uaed8 \ubd80\ub974\uc9d6\uc5b4 \uac00\ub85c\ub418 \uc5ec\ud638\uc640\uc5ec \uad6c\ud558\uace0 \uad6c\ud558\uc624\ub2c8 \uc774 \uc0ac\ub78c\uc758 \uc0dd\uba85 \uae4c\ub2ed\uc5d0 \uc6b0\ub9ac\ub97c \uba78\ub9dd\uc2dc\ud0a4\uc9c0 \ub9c8\uc635\uc18c\uc11c \ubb34\uc8c4\ud55c \ud53c\ub97c \uc6b0\ub9ac\uc5d0\uac8c \ub3cc\ub9ac\uc9c0 \ub9c8\uc635\uc18c\uc11c \uc8fc \uc5ec\ud638\uc640\uaed8\uc11c\ub294 \uc8fc\uc758 \ub73b\ub300\ub85c \ud589\ud558\uc2ec\uc774\ub2c8\uc774\ub2e4 \ud558\uace0", "content_old": "14 \ubb34\ub9ac\uac00 \uc5ec\ud638\uc640\uaed8 \ubd80\ub974\uc9d6\uc5b4 \uac00\ub85c\ub418 \uc5ec\ud638\uc640\uc5ec \uad6c\ud558\uace0 \uad6c\ud558\uc624\ub2c8 \uc774 \uc0ac\ub78c\uc758 \uc0dd\uba85 \uae4c\ub2ed\uc5d0 \uc6b0\ub9ac\ub97c \uba78\ub9dd\uc2dc\ud0a4\uc9c0 \ub9c8\uc635\uc18c\uc11c \ubb34\uc8c4\ud55c \ud53c\ub97c \uc6b0\ub9ac\uc5d0\uac8c \ub3cc\ub9ac\uc9c0 \ub9c8\uc635\uc18c\uc11c \uc8fc \uc5ec\ud638\uc640\uaed8\uc11c\ub294 \uc8fc\uc758 \ub73b\ub300\ub85c \ud589\ud558\uc2ec\uc774\ub2c8\uc774\ub2e4 \ud558\uace0", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 15, "content": "\uc694\ub098\ub97c \ub4e4\uc5b4 \ubc14\ub2e4\uc5d0 \ub358\uc9c0\ub9e4 \ubc14\ub2e4\uc758 \ub6f0\ub178\ub294 \uac83\uc774 \uace7 \uadf8\uce5c\uc9c0\ub77c", "content_old": "15 \uc694\ub098\ub97c \ub4e4\uc5b4 \ubc14\ub2e4\uc5d0 \ub358\uc9c0\ub9e4 \ubc14\ub2e4\uc758 \ub6f0\ub178\ub294 \uac83\uc774 \uace7 \uadf8\uce5c\uc9c0\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 16, "content": "\uadf8 \uc0ac\ub78c\ub4e4\uc774 \uc5ec\ud638\uc640\ub97c \ud06c\uac8c \ub450\ub824\uc6cc\ud558\uc5ec \uc5ec\ud638\uc640\uaed8 \uc81c\ubb3c\uc744 \ub4dc\ub9ac\uace0 \uc11c\uc6d0\uc744 \ud558\uc600\ub354\ub77c", "content_old": "16 \uadf8 \uc0ac\ub78c\ub4e4\uc774 \uc5ec\ud638\uc640\ub97c \ud06c\uac8c \ub450\ub824\uc6cc\ud558\uc5ec \uc5ec\ud638\uc640\uaed8 \uc81c\ubb3c\uc744 \ub4dc\ub9ac\uace0 \uc11c\uc6d0\uc744 \ud558\uc600\ub354\ub77c", "url": "https://www.wordproject.org/bibles/kr/32/1.htm", "lang": "ko", "checked": 1}, {"bible_type": 0, "book": "\uc694\ub098", "chapter": "1", "line": 17, "content": "\uc5ec\ud638\uc640\uaed8\uc11c \uc774\ubbf8 \ud070 \ubb3c\uace0\uae30\ub97c \uc608\ube44\ud558\uc0ac \uc694\ub098\ub97c \uc0bc\ud0a4\uac8c \ud558\uc168\uc73c\ubbc0\ub85c \uc694\ub098\uac00 \uc0bc\uc77c \uc0bc\uc57c\ub97c \ubb3c\uace0\uae30 \ubc30\uc5d0 \uc788\uc73c\ub2c8\ub77c", "content_old": "17 \uc5ec\ud638\uc640\uaed8\uc11c \uc774\ubbf8 \ud070 \ubb3c\uace0\uae30\ub97c \uc608\ube44\ud558\uc0ac \uc694\ub098\ub97c \uc0bc\ud0a4\uac8c \ud558\uc168\uc73c\ubbc0\